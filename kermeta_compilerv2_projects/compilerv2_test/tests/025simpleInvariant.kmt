@mainClass "constraint_testcases::Main"
@mainOperation "main"


package constraint_testcases;


require kermeta

using kermeta::standard
class Main
{
	operation main() : Void is do 
		var v : superMain2 init superMain2.new
		
			v.superstr2 := "supertiti2"
		do
		v.checkInvariants
		v.superstr2 := "supertiti1"
		v.checkInvariants
		rescue ( e : kermeta::exceptions::Exception) 
			stdio.writeln("contraintes viol√©es")
		end
		
		
	end
}
class superMain2
{
	reference superstr2 : String
	
	inv superMain2Inv is do 
		stdio.writeln( "Inv superMain2Inv : " + superstr2 )
		superstr2 == "supertiti2" 
	end
}