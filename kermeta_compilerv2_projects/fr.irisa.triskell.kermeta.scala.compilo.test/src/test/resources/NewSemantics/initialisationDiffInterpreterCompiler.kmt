@mainClass "pkgDiffInterpCompilInit::DiffInterpCompilInit"
@mainOperation "main"


package pkgDiffInterpCompilInit;


require kermeta
using kermeta::standard

class DiffInterpCompilInit
{
	operation main() : Void is do
	// Interpreter: launches an exception
	// Compiler: goes into else
		var b : Boolean
	
		do
			if(true and b) then
				stdio.writeln("if")
			else
				stdio.writeln("else")
			end
		rescue(e : kermeta::exceptions::RuntimeError)
			stdio.writeln("exception")
		end
		
		do
			if(false and b) then
				stdio.writeln("if")
			else
				stdio.writeln("else")
			end
		rescue(e : kermeta::exceptions::RuntimeError)
			stdio.writeln("exception")
		end
	end
}