@mainClass "p1::Main"
@mainOperation "main"


package p1;


require kermeta
class Main
{
	operation main() : Void is do 
		var e : kermeta::exceptions::Exception init kermeta::exceptions::Exception.new 
		raise e
		rescue(e : kermeta::exceptions::Exception)
		stdio.writeln("exception caught")
	end
}