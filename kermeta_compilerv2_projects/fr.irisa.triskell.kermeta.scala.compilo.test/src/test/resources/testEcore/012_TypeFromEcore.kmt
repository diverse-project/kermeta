@mainClass "test1::Main"
@mainOperation "main"


package test1;


require kermeta
require "platform:/resource/fr.irisa.triskell.kermeta.scala.compilo.test/src/test/resources/testEcore/A.ecore"
 
using kermeta::standard
using kermeta::persistence
class Main
{
	operation main() : Void is do 
		var a : A init A.new
		a.name := "Bonjour"
		a.test		
	end
}

aspect class A{
	
	
	attribute  toto : String
	
	operation test() : Void is do
	self.toto := "toto"
		stdio.writeln(""+self.name + " " + self.toto ) 
	end
	
	
}

package foo{
	class C inherits test1::Main {
		attribute  toto : String
		attribute  m : test1::Main
		attribute  m1 : test1::A
		
		operation test(toto : String,m : test1::Main,m1 : test1::A):test1::A is do
			result := void
		end
		)
		
	}
	class D inherits test1::A inherits C{
		attribute  toto : String
		attribute  m : test1::Main
		attribute  m1 : test1::A
		operation test1(toto : String,m : test1::Main,m1 : test1::A):test1::Main is do
			result := void
		end
		
	}

}
