@mainClass "test1::Main"
@mainOperation "main"


package test1;


require kermeta
require "platform:/resource/fr.irisa.triskell.kermeta.scala.compilo.test/src/test/resources/testEcore/A.ecore"
 
using kermeta::standard
using kermeta::persistence
class Main
{
	operation main() : Void is do 
		var a : A init A.new
		a.name := "Bonjour"
		a.test
		
	end
}

aspect class A{
	
	
	attribute  toto : String
	
	operation test() : Void is do
	self.toto := "toto"
		stdio.writeln(""+self.name + " " + self.toto ) 
	end
	
	
}

package foo{
class C{
	attribute  toto : String
}
}
