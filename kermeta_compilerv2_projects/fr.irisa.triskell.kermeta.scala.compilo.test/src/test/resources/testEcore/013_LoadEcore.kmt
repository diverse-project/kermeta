@mainClass "test1::Main"
@mainOperation "main"


package test1;


require kermeta
require "platform:/resource/fr.irisa.triskell.kermeta.scala.compilo.test/resources/testEcore/A.ecore"

using kermeta::standard
using kermeta::persistence
class Main
{
	operation main() : Void is do 
 		 var repository : EMFRepository init EMFRepository.new
		 var resource : EMFResource 
 		resource ?= repository.getResource("platform:/resource/fr.irisa.triskell.kermeta.scala.compilo.test/resources/testEcore/A.xmi")//, "platform:/resource/KermetaCompilerTestCase/src/test1/A.ecore")
 		resource.load
 		var a : A 
 		a?= resource.contents.one
 		a.test
		
	end
}

aspect class A{
	
	
	attribute  toto : String
	
	operation test() : Void is do
	self.toto := "toto"
		stdio.writeln(self.name + " " + self.toto ) 
	end
	
	
}