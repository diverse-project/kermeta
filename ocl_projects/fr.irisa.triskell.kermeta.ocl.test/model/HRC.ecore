            eOpposite="#//level0/behaviors/BlockPin/block"/>
        <eStructuralFeatures xsi:type="ecore:EReference" name="variable" upperBound="-1"
            eType="#//level0/structures/Variable" containment="true" eOpposite="#//level0/structures/Variable/block"/>
        <eStructuralFeatures xsi:type="ecore:EReference" name="implementation" eType="#//level0/behaviors/Implementation"
            eOpposite="#//level0/behaviors/Implementation/block"/>
      </eClassifiers>
