            eType="#//level0/behaviors/HRCBlock" transient="true" eOpposite="#//level0/behaviors/HRCBlock/pin"/>
        <eStructuralFeatures xsi:type="ecore:EReference" name="type" lowerBound="1"
            eType="#//level0/expressions/DataType"/>
      </eClassifiers>
      <eClassifiers xsi:type="ecore:EClass" name="ReferencePin" eSuperTypes="#//level0/behaviors/Pin">
