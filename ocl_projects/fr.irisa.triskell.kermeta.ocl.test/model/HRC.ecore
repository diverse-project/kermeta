            eType="#//level0/behaviors/Transition" containment="true" eOpposite="#//level0/behaviors/Transition/stateMachine"/>
        <eStructuralFeatures xsi:type="ecore:EReference" name="initialState" lowerBound="1"
            eType="#//level0/behaviors/State"/>
        <eStructuralFeatures xsi:type="ecore:EReference" name="block" eType="#//level0/behaviors/HybridAutomatonBlock"
            transient="true" eOpposite="#//level0/behaviors/HybridAutomatonBlock/behavior"/>
        <eStructuralFeatures xsi:type="ecore:EReference" name="owningState" eType="#//level0/behaviors/State"
            transient="true" eOpposite="#//level0/behaviors/State/submachine"/>
      </eClassifiers>
      <eClassifiers xsi:type="ecore:EClass" name="State" eSuperTypes="#//level0/structures/NamedElement">
        <eStructuralFeatures xsi:type="ecore:EReference" name="stateMachine" lowerBound="1"
            eType="#//level0/behaviors/StateMachine" transient="true" eOpposite="#//level0/behaviors/StateMachine/state"/>
        <eStructuralFeatures xsi:type="ecore:EReference" name="outgoing" upperBound="-1"
            eType="#//level0/behaviors/Transition" eOpposite="#//level0/behaviors/Transition/source"/>
        <eStructuralFeatures xsi:type="ecore:EReference" name="incoming" upperBound="-1"
            eType="#//level0/behaviors/Transition" eOpposite="#//level0/behaviors/Transition/target"/>
        <eStructuralFeatures xsi:type="ecore:EReference" name="stateInvariant" eType="#//level0/expressions/Expression"
            containment="true"/>
        <eStructuralFeatures xsi:type="ecore:EReference" name="submachine" eType="#//level0/behaviors/StateMachine"
            containment="true" eOpposite="#//level0/behaviors/StateMachine/owningState"/>
      </eClassifiers>
      <eClassifiers xsi:type="ecore:EClass" name="Transition">
        <eStructuralFeatures xsi:type="ecore:EReference" name="stateMachine" lowerBound="1"
            eType="#//level0/behaviors/StateMachine" transient="true" eOpposite="#//level0/behaviors/StateMachine/transition"/>
        <eStructuralFeatures xsi:type="ecore:EReference" name="source" lowerBound="1"
            eType="#//level0/behaviors/State" eOpposite="#//level0/behaviors/State/outgoing"/>
        <eStructuralFeatures xsi:type="ecore:EReference" name="target" lowerBound="1"
            eType="#//level0/behaviors/State" eOpposite="#//level0/behaviors/State/incoming"/>
        <eStructuralFeatures xsi:type="ecore:EReference" name="effect" eType="#//level0/actionspecifications/ActionSpecification"
            containment="true"/>
        <eStructuralFeatures xsi:type="ecore:EReference" name="guard" eType="#//level0/expressions/Expression"
            containment="true"/>
      </eClassifiers>
      <eClassifiers xsi:type="ecore:EClass" name="HRCBlock" abstract="true" eSuperTypes="#//level0/structures/NamedElement">
