        <eStructuralFeatures xsi:type="ecore:EReference" name="value" lowerBound="1"
            eType="#//level0/expressions/Expression" containment="true"/>
        <eStructuralFeatures xsi:type="ecore:EReference" name="caseAction" lowerBound="1"
            eType="#//level0/actionspecifications/ActionSpecification" containment="true"/>
      </eClassifiers>
      <eClassifiers xsi:type="ecore:EClass" name="Assignment" eSuperTypes="#//level0/actionspecifications/ActionSpecification">
        <eStructuralFeatures xsi:type="ecore:EReference" name="value" lowerBound="1"
