            eOpposite="#//level0/behaviors/Contract/assumption"/>
        <eStructuralFeatures xsi:type="ecore:EReference" name="promisingContract"
            eType="#//level0/behaviors/Contract" eOpposite="#//level0/behaviors/Contract/promise"/>
        <eStructuralFeatures xsi:type="ecore:EReference" name="context" lowerBound="1"
            eType="#//level0/structures/RichComponent" transient="true" eOpposite="#//level0/structures/RichComponent/block"/>
