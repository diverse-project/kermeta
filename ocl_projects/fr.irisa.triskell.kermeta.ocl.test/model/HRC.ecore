        <eStructuralFeatures xsi:type="ecore:EReference" name="content" upperBound="-1"
            eType="#//level0/actionspecifications/ActionSpecification" containment="true"/>
        <eStructuralFeatures xsi:type="ecore:EReference" name="declaredVariable" upperBound="-1"
            eType="#//level0/structures/Variable" containment="true"/>
        <eStructuralFeatures xsi:type="ecore:EReference" name="declaredDataType" upperBound="-1"
            eType="#//level0/expressions/DataType" containment="true"/>
      </eClassifiers>
      <eClassifiers xsi:type="ecore:EClass" name="ControlFlowAction" abstract="true"
          eSuperTypes="#//level0/actionspecifications/ActionSpecification"/>
      <eClassifiers xsi:type="ecore:EClass" name="JumpAction" abstract="true" eSuperTypes="#//level0/actionspecifications/ControlFlowAction"/>
      <eClassifiers xsi:type="ecore:EClass" name="IfAction" eSuperTypes="#//level0/actionspecifications/ConditionalAction">
        <eStructuralFeatures xsi:type="ecore:EReference" name="condition" lowerBound="1"
            eType="#//level0/expressions/Expression" containment="true"/>
        <eStructuralFeatures xsi:type="ecore:EReference" name="ifCase" lowerBound="1"
            eType="#//level0/actionspecifications/ActionSpecification" containment="true"/>
        <eStructuralFeatures xsi:type="ecore:EReference" name="elseCase" eType="#//level0/actionspecifications/ActionSpecification"
            containment="true"/>
      </eClassifiers>
      <eClassifiers xsi:type="ecore:EClass" name="LoopAction" abstract="true" eSuperTypes="#//level0/actionspecifications/ControlFlowAction">
        <eStructuralFeatures xsi:type="ecore:EReference" name="condition" lowerBound="1"
            eType="#//level0/expressions/Expression" containment="true"/>
        <eStructuralFeatures xsi:type="ecore:EReference" name="content" lowerBound="1"
            eType="#//level0/actionspecifications/ActionSpecification" containment="true"/>
      </eClassifiers>
      <eClassifiers xsi:type="ecore:EClass" name="SwitchAction" eSuperTypes="#//level0/actionspecifications/ConditionalAction">
        <eStructuralFeatures xsi:type="ecore:EReference" name="defaultAction" eType="#//level0/actionspecifications/ActionSpecification"
            containment="true"/>
        <eStructuralFeatures xsi:type="ecore:EReference" name="switchExpression" lowerBound="1"
            eType="#//level0/expressions/Expression" containment="true"/>
        <eStructuralFeatures xsi:type="ecore:EReference" name="case" upperBound="-1"
            eType="#//level0/actionspecifications/SwitchCase" containment="true"/>
      </eClassifiers>
      <eClassifiers xsi:type="ecore:EClass" name="WhileDoAction" eSuperTypes="#//level0/actionspecifications/LoopAction"/>
      <eClassifiers xsi:type="ecore:EClass" name="ForAction" eSuperTypes="#//level0/actionspecifications/LoopAction">
        <eStructuralFeatures xsi:type="ecore:EReference" name="iteration" lowerBound="1"
            eType="#//level0/actionspecifications/ActionSpecification" containment="true"/>
        <eStructuralFeatures xsi:type="ecore:EReference" name="initialization" lowerBound="1"
            eType="#//level0/actionspecifications/ActionSpecification" containment="true"/>
      </eClassifiers>
      <eClassifiers xsi:type="ecore:EClass" name="ConditionalAction" abstract="true"
          eSuperTypes="#//level0/actionspecifications/ActionSpecification #//level0/actionspecifications/ControlFlowAction"/>
