  <eSubpackages name="level0" nsURI="http:///hrc/level0.ecore" nsPrefix="hrc.level0">
    <eSubpackages name="actionspecifications" nsURI="http:///hrc/level0/actionspecifications.ecore"
        nsPrefix="hrc.level0.actionspecifications">
      <eClassifiers xsi:type="ecore:EClass" name="ActionSpecification" abstract="true"/>
