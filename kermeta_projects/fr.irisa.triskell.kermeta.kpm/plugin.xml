        id="kermetaProjectBuilder"
        point="org.eclipse.core.resources.builders">
     <builder>
        <run class="fr.irisa.triskell.kermeta.kpm.builder.KermetaProjectBuilder"/>
     </builder>
  </extension>-->
  <extension
        point="org.eclipse.ui.startup">
     <startup class="fr.irisa.triskell.kermeta.kpm.plugin.KPMStartup"/>
  </extension>
  <extension
        point="org.eclipse.ui.propertyPages">
     <page
           class="fr.irisa.triskell.kermeta.kpm.properties.KPMPropertyPage"
           id="fr.irisa.triskell.kermeta.kpm.page1"
           name="Kermeta"
           objectClass="org.eclipse.core.resources.IProject">
        <filter
         	name="projectNature"
         	value="fr.irisa.triskell.kermeta.KermetaNature">
      	</filter>
     </page>
  </extension>



</plugin>

