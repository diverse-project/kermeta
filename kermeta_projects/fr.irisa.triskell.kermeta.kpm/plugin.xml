<plugin>
   <extension-point id="javaAction" name="JavaAction" schema="schema/javaAction.exsd"/>
   <extension-point id="kermetaAction" name="KermetaAction" schema="schema/kermetaAction.exsd"/>
   <extension-point id="antAction" name="antAction" schema="schema/antAction.exsd"/>

  <extension point="org.eclipse.emf.ecore.generated_package">
    <package 
       uri = "http://kpm" 
       class = "fr.irisa.triskell.kermeta.kpm.KpmPackage"
       genModel = "model/kpm.genmodel" /> 
  </extension>
