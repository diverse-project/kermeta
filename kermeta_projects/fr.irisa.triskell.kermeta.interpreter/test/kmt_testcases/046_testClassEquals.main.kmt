@mainClass "testClassEquals::TestClassEquals"
@mainOperation "operation1"

package testClassEquals;

require kermeta

using kermeta::language::structure

class TestClassEquals inherits kermeta::kunit::Test {

    operation testStringEquals() : Void is do
        assertTrue("toto".getMetaClass == "titi".getMetaClass)
    end

    operation testStringEquals2() : Void is do
    	var c : Class init Class.new
    	c.typeDefinition ?= "titi".getMetaClass.typeDefinition
        assertTrue("toto".getMetaClass == c)
    end


}

