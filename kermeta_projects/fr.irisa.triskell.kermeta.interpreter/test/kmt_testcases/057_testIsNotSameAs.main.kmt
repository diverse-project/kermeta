@mainClass "Object::Test"
@mainOperation "main"

package Object;

require kermeta

using kermeta::standard
using kermeta::utils
using kermeta::language::structure

abstract class Bidon<G> {
}

class SubBidon<G> inherits Bidon<G> {
}

class Riri {
}
class Loulou inherits Riri {}
class Fifi<G> inherits Riri {}

class Test inherits kermeta::kunit::Test {

	operation main() : Void is do
	end
	
	operation testCorrectIsNotSameAs1() : Boolean is do
		var i1 : Integer init 10
		var i2 : Integer init 10
		assert (not i1.isNotSameAs(i2))
		result := not i1.isNotSameAs(i2)
	end

	operation testCorrectIsNotSameAs2() : Boolean  is do
		var i1 : Integer init 10
		var i2 : Integer
		i2 := i1
		assert (not i1.isNotSameAs(i2))
		result := not i1.isNotSameAs(i2)
	end

	operation testIncorrectIsNotSameAs1() : Boolean  is do
		var i1 : Riri init Riri.new
		var i2 : Riri init Riri.new
		assert ( i1.isNotSameAs(i2))
		result := i1.isNotSameAs(i2)
	end

	operation testCorrectIsNotSameAs4() : Boolean  is do
		var i1 : Riri init Riri.new
		var i2 : Riri
		i2 := i1
		assert ( not i1.isNotSameAs(i2))
		result := not i1.isNotSameAs(i2)
	end

	operation testCorrectIsNotSameAs5() : Boolean  is do
		var i1 : Fifi<Riri> init Fifi<Riri>.new
		var i2 : Fifi<Riri>
		i2 := i1
		assert ( not i1.isNotSameAs(i2))
		result := not i1.isNotSameAs(i2)
	end
	
	operation testCorrectIsNotSameAs6() : Boolean  is do
		var i1 : Fifi<Riri> init Fifi<Riri>.new
		var i2 : Fifi<Riri> init Fifi<Riri>.new
		assert ( i1.isNotSameAs(i2))
		result := i1.isNotSameAs(i2)
	end
}