package testLoop;

require "../../../fr.irisa.triskell.kermeta.framework/src/kermeta/Standard.kmt"


// call an operation from another package 
class TestLoop {

    operation operation1() : kermeta::standard::Integer is do
		var v1 : kermeta::standard::Integer init 3
		var v2 : kermeta::standard::Integer init 6
		
		from var i : kermeta::standard::Integer init 0
		until i == 10
		loop
			v1 := v1*v1 + v2
			i := i + 1
		end
		result := v1
    end
}
