package testOpCallObject;

require "../../../fr.irisa.triskell.kermeta.framework/src/kermeta/Standard.kmt"

// Creation of an object
class MyObject {

	attribut attr1 : Integer
	attribut attr2 : Boolean

	operation set_attr1(arg0 : Integer) : Void is do
	
		attr1 := arg0
	end
	
	operation set_attr2(arg0 : Boolean) : Void is do

		attr2 := arg0
	end

}

class MyObjectCreation {

	operation createMyObject() : MyObject is do
		var object : MyObject
		var bool   : Boolean
		var int    : Integer
		bool := true
		int  := 4
		object := MyObject.new
		object.set_attr1(int)
		object.set_attr2(bool)
		result := object
	end
}