//@mainClass "root_package::MainToBeRequired"
//@mainOperation "mainToBeRequired"



package root_package;
require kermeta
using kermeta::standard

class MainToBeRequired
{

	reference strRequired : String 
	reference b : Boolean
	
	inv linvRequired is b
    operation mainToBeRequired() : Void is do 
    	strRequired := "trucRequiredPre"
    	b := true
       	truc("trucRequiredPost")

    end
    
    
    operation truc(param : String)
	pre lapreRequired is do strRequired == "trucRequiredPre" end
	post lapostRequired is strRequired == "trucRequiredPost" 
	is
	do
        self.checkInvariants
		strRequired := param
        stdio.writeln("test_require")
        
	end
	
}