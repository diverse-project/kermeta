        <details key="documentation" value="/**&#xA; * Condition is the model-element representation for the &lt;code>&lt;/code> &#xA; */"/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EReference" name="thenBody" lowerBound="1"
