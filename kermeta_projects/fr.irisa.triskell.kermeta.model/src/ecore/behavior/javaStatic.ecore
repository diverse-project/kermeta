<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="ftm">
  <eSubpackages name="behavior">
    <eClassifiers xsi:type="ecore:EClass" name="JavaStaticCall" eSuperTypes="#//behavior/Expression">
      <eAnnotations source="kermeta">
        <details key="documentation" value="/**&#xA; * This class represent a call to an extern Java operation. &#xA; * This Java operation must be static and have at least one RuntimeO&#xA; */"/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EReference" name="parameters" upperBound="-1"
          eType="#//behavior/Expression" containment="true">
        <eAnnotations source="kermeta">
          <details key="documentation" value="/**&#xA; * Parameters of the Java static call provided as KerMeta expressions&#xA; */"/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="jclass" eType="#//structure/String">
        <eAnnotations source="kermeta">
          <details key="documentation" value="/**&#xA; * Qualified name of the targeted Java class&#xA; */"/>
        </eAnnotations>
      </eStructuralFeatures>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="jmethod" eType="#//structure/String">
        <eAnnotations source="kermeta">
          <details key="documentation" value="/**&#xA; * Name of the Java method to be launched in the identified Java class&#xA; */"/>
        </eAnnotations>
      </eStructuralFeatures>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Expression" abstract="true"/>
  </eSubpackages>
  <eSubpackages name="structure">
    <eClassifiers xsi:type="ecore:EDataType" name="String" instanceClassName="java.lang.String"/>
  </eSubpackages>
</ecore:EPackage>
