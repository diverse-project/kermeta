<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="ftm">
  <eSubpackages name="behavior">
    <eClassifiers xsi:type="ecore:EClass" name="LambdaExpression" eSuperTypes="#//behavior/Expression">
      <eStructuralFeatures xsi:type="ecore:EReference" name="parameters" upperBound="-1"
          eType="#//behavior/LambdaParameter" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="body" eType="#//behavior/Expression"
          containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Expression" abstract="true"/>
    <eClassifiers xsi:type="ecore:EClass" name="LambdaParameter" eSuperTypes="#//structure/Object">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="#//structure/String"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="type" eType="#//behavior/TypeReference"
          containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="TypeReference"/>
  </eSubpackages>
  <eSubpackages name="structure">
    <eClassifiers xsi:type="ecore:EDataType" name="String" instanceClassName="java.lang.String"/>
    <eClassifiers xsi:type="ecore:EClass" name="Object"/>
  </eSubpackages>
</ecore:EPackage>
