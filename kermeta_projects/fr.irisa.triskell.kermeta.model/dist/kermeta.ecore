<?xml version="1.0" encoding="ASCII"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="kermeta" nsURI="http://kermeta/kermeta.ecore" nsPrefix="kermeta">
  <eSubpackages name="behavior" nsURI="http://kermeta_behavior/kermeta.ecore" nsPrefix="kermeta_behavior">
    <eClassifiers xsi:type="ecore:EClass" name="CallExpression" abstract="true" eSuperTypes="//behavior/Expression">
      <eStructuralFeatures xsi:type="ecore:EReference" name="parameters" upperBound="-1" eType="//behavior/Expression" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="//structure/String"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Expression" abstract="true" eSuperTypes="//structure/Object"/>
    <eClassifiers xsi:type="ecore:EClass" name="CallVariable" eSuperTypes="//behavior/CallExpression"/>
    <eClassifiers xsi:type="ecore:EClass" name="CallFeature" eSuperTypes="//behavior/CallExpression">
      <eStructuralFeatures xsi:type="ecore:EReference" name="target" eType="//behavior/Expression" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="CallSuperOperation" eSuperTypes="//behavior/CallExpression"/>
    <eClassifiers xsi:type="ecore:EClass" name="CallResult" eSuperTypes="//behavior/CallVariable"/>
    <eClassifiers xsi:type="ecore:EClass" name="Assignement" eSuperTypes="//behavior/Expression">
      <eStructuralFeatures xsi:type="ecore:EReference" name="target" lowerBound="1" eType="//behavior/CallExpression" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="value" lowerBound="1" eType="//behavior/Expression" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="isCast" eType="//structure/Boolean"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Block" eSuperTypes="//behavior/Expression">
      <eStructuralFeatures xsi:type="ecore:EReference" name="statement" lowerBound="1" upperBound="-1" eType="//behavior/Expression" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="rescueBlock" upperBound="-1" eType="//behavior/Rescue" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Conditionnal" eSuperTypes="//behavior/Expression">
      <eStructuralFeatures xsi:type="ecore:EReference" name="thenBody" eType="//behavior/Expression" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="elseBody" eType="//behavior/Expression" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="condition" eType="//behavior/Expression" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Literal" abstract="true" eSuperTypes="//behavior/Expression"/>
    <eClassifiers xsi:type="ecore:EClass" name="EmptyExpression" eSuperTypes="//behavior/Expression"/>
    <eClassifiers xsi:type="ecore:EClass" name="IntegerLiteral" eSuperTypes="//behavior/Literal">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" eType="//structure/Integer"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="StringLiteral" eSuperTypes="//behavior/Literal">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" eType="//structure/String"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="BooleanLiteral" eSuperTypes="//behavior/Literal">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" eType="//structure/Boolean"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="TypeLiteral" eSuperTypes="//behavior/Literal">
      <eStructuralFeatures xsi:type="ecore:EReference" name="typeref" lowerBound="1" eType="//behavior/TypeReference" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="TypeReference" eSuperTypes="//structure/MultiplicityElement"/>
    <eClassifiers xsi:type="ecore:EClass" name="VoidLiteral" eSuperTypes="//behavior/Literal"/>
    <eClassifiers xsi:type="ecore:EClass" name="Loop" eSuperTypes="//behavior/Expression">
      <eStructuralFeatures xsi:type="ecore:EReference" name="initiatization" eType="//behavior/Expression" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="body" eType="//behavior/Expression" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="stopCondition" eType="//behavior/Expression" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="SelfExpression" eSuperTypes="//behavior/Expression"/>
    <eClassifiers xsi:type="ecore:EClass" name="VariableDecl" eSuperTypes="//behavior/Expression">
      <eStructuralFeatures xsi:type="ecore:EReference" name="initialization" lowerBound="1" eType="//behavior/Expression" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="identifier" eType="//structure/String"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="type" eType="//behavior/TypeReference" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Raise" eSuperTypes="//behavior/Expression">
      <eStructuralFeatures xsi:type="ecore:EReference" name="expression" lowerBound="1" eType="//behavior/Expression" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Rescue" eSuperTypes="//structure/Object">
      <eStructuralFeatures xsi:type="ecore:EReference" name="body" lowerBound="1" upperBound="-1" eType="//behavior/Expression" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="exceptionType" lowerBound="1" eType="//behavior/TypeReference" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="exceptionName" eType="//structure/String"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="JavaStaticCall" eSuperTypes="//behavior/Expression">
      <eStructuralFeatures xsi:type="ecore:EReference" name="parameters" upperBound="-1" eType="//behavior/Expression" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="jclass" eType="//structure/String"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="jmethod" eType="//structure/String"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="LambdaExpression" eSuperTypes="//behavior/Expression">
      <eStructuralFeatures xsi:type="ecore:EReference" name="parameters" upperBound="-1" eType="//behavior/LambdaParameter" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="body" eType="//behavior/Expression" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="LambdaParameter" eSuperTypes="//structure/Object">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="//structure/String"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="type" eType="//behavior/TypeReference" containment="true"/>
    </eClassifiers>
  </eSubpackages>
  <eSubpackages name="structure" nsURI="http://kermeta_structure/kermeta.ecore" nsPrefix="kermeta_structure">
    <eClassifiers xsi:type="ecore:EDataType" name="String" instanceClassName="kermeta::standard::String"/>
    <eClassifiers xsi:type="ecore:EClass" name="Object">
      <eOperations name="getMetaClass" eType="//structure/Class"/>
      <eOperations name="container" eType="//structure/Object"/>
      <eOperations name="equals" eType="//structure/Boolean">
        <eParameters name="element" eType="//structure/Object"/>
      </eOperations>
      <eOperations name="get" eType="//structure/Object">
        <eParameters name="property" eType="//structure/Property"/>
      </eOperations>
      <eOperations name="set">
        <eParameters name="property" eType="//structure/Property"/>
        <eParameters name="element" eType="//structure/Object"/>
      </eOperations>
      <eOperations name="isSet" eType="//structure/Boolean">
        <eParameters name="property" eType="//structure/Property"/>
      </eOperations>
      <eOperations name="unset">
        <eParameters name="property" eType="//structure/Property"/>
      </eOperations>
      <eStructuralFeatures xsi:type="ecore:EReference" name="tag" upperBound="-1" eType="//structure/Tag" eOpposite="//structure/Tag/object"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EDataType" name="Boolean" instanceClassName="kermeta::standard::Boolean"/>
    <eClassifiers xsi:type="ecore:EDataType" name="Integer" instanceClassName="kermeta::standard::Integer"/>
    <eClassifiers xsi:type="ecore:EClass" name="Class" eSuperTypes="//structure/Type">
      <eOperations name="new" eType="//structure/Object"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="classDefinition" lowerBound="1" eType="//structure/ClassDefinition"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="typeParamBinding" upperBound="-1" eType="//structure/TypeVariableBinding" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="ownedAttribute" upperBound="-1" eType="//structure/Property" changeable="false" derived="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="ownedOperation" upperBound="-1" eType="//structure/Operation" changeable="false" derived="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="superClass" upperBound="-1" eType="//structure/Class" changeable="false" derived="true"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="isAbstract" eType="//structure/Boolean" changeable="false" defaultValueLiteral="false" derived="true"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="//structure/String" changeable="false" derived="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="MultiplicityElement" eSuperTypes="//structure/TypedElement">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="isOrdered" eType="//structure/Boolean" defaultValueLiteral="false"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="isUnique" eType="//structure/Boolean" defaultValueLiteral="true"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="lower" eType="//structure/Integer"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="upper" eType="//structure/UnlimitedNatural"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Operation" eSuperTypes="//structure/MultiplicityElement">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="isAbstract" eType="//structure/Boolean"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="body" eType="//behavior/Expression" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="superOperation" eType="//structure/Operation"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="owningClass" eType="//structure/ClassDefinition" eOpposite="//structure/ClassDefinition/ownedOperation"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="typeParameter" upperBound="-1" eType="//structure/TypeVariable"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="raisedException" upperBound="-1" eType="//structure/Type"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="ownedParameter" upperBound="-1" eType="//structure/Parameter" containment="true" eOpposite="//structure/Parameter/operation"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Tag" eSuperTypes="//structure/Object">
      <eStructuralFeatures xsi:type="ecore:EReference" name="object" lowerBound="1" upperBound="-1" eType="//structure/Object" eOpposite="//structure/Object/tag"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="//structure/String"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" eType="//structure/String"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Property" eSuperTypes="//structure/MultiplicityElement">
      <eStructuralFeatures xsi:type="ecore:EReference" name="getterbody" eType="//behavior/Expression" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="setterbody" eType="//behavior/Expression" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="owningClass" eType="//structure/ClassDefinition" eOpposite="//structure/ClassDefinition/ownedAttributes"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="opposite" eType="//structure/Property"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="isReadOnly" eType="//structure/Boolean" defaultValueLiteral="false"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="default" eType="//structure/String"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="isComposite" eType="//structure/Boolean" defaultValueLiteral="false"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="isDerived" eType="//structure/Boolean" defaultValueLiteral="false"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="isID" eType="//structure/Boolean"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="TypeContainer" abstract="true" eSuperTypes="//structure/Object">
      <eStructuralFeatures xsi:type="ecore:EReference" name="containedType" upperBound="-1" eType="//structure/Type" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ClassDefinition" eSuperTypes="//structure/TypeContainer //structure/TypeDefinition">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="isAbstract" eType="//structure/Boolean"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="ownedAttributes" upperBound="-1" eType="//structure/Property" containment="true" eOpposite="//structure/Property/owningClass"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="ownedOperation" upperBound="-1" eType="//structure/Operation" containment="true" eOpposite="//structure/Operation/owningClass"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="superType" upperBound="-1" eType="//structure/Type"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="typeParameter" upperBound="-1" eType="//structure/TypeVariable" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="TypedElement" eSuperTypes="//structure/TypeContainer //structure/NamedElement">
      <eStructuralFeatures xsi:type="ecore:EReference" name="type" eType="//structure/Type"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="TypeVariable" eSuperTypes="//structure/TypeContainer //structure/Type //structure/NamedElement">
      <eStructuralFeatures xsi:type="ecore:EReference" name="supertype" eType="//structure/Type"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ProductType" eSuperTypes="//structure/TypeContainer //structure/Type">
      <eStructuralFeatures xsi:type="ecore:EReference" name="type" lowerBound="1" upperBound="-1" eType="//structure/Type"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="FunctionType" eSuperTypes="//structure/TypeContainer //structure/Type">
      <eStructuralFeatures xsi:type="ecore:EReference" name="left" eType="//structure/Type"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="right" eType="//structure/Type"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Type" eSuperTypes="//structure/Object">
      <eOperations name="isInstance" eType="//structure/Boolean">
        <eParameters name="element" eType="//structure/Object"/>
      </eOperations>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="TypeVariableBinding" eSuperTypes="//structure/TypeContainer //structure/Object">
      <eStructuralFeatures xsi:type="ecore:EReference" name="variable" lowerBound="1" eType="//structure/TypeVariable"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="type" lowerBound="1" eType="//structure/Type"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="PrimitiveType" eSuperTypes="//structure/TypeContainer //structure/DataType">
      <eStructuralFeatures xsi:type="ecore:EReference" name="instanceType" eType="//structure/Type"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="NamedElement" eSuperTypes="//structure/Object">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="//structure/String"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="SelfType" eSuperTypes="//structure/Type"/>
    <eClassifiers xsi:type="ecore:EClass" name="Package" eSuperTypes="//structure/NamedElement">
      <eStructuralFeatures xsi:type="ecore:EReference" name="ownedTypeDefinition" upperBound="-1" eType="//structure/TypeDefinition" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="nestedPackage" upperBound="-1" eType="//structure/Package" containment="true" eOpposite="//structure/Package/nestingPackage"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="nestingPackage" eType="//structure/Package" transient="true" eOpposite="//structure/Package/nestedPackage"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="uri" eType="//structure/String"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="TypeDefinition" eSuperTypes="//structure/NamedElement"/>
    <eClassifiers xsi:type="ecore:EClass" name="VoidType" eSuperTypes="//structure/Type"/>
    <eClassifiers xsi:type="ecore:EClass" name="DataType" eSuperTypes="//structure/TypeDefinition //structure/Type"/>
    <eClassifiers xsi:type="ecore:EClass" name="Enumeration" eSuperTypes="//structure/DataType">
      <eStructuralFeatures xsi:type="ecore:EReference" name="ownedLiteral" upperBound="-1" eType="//structure/EnumerationLiteral" containment="true" eOpposite="//structure/EnumerationLiteral/enumeration"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="EnumerationLiteral" eSuperTypes="//structure/NamedElement">
      <eStructuralFeatures xsi:type="ecore:EReference" name="enumeration" eType="//structure/Enumeration" transient="true" eOpposite="//structure/Enumeration/ownedLiteral"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Parameter" eSuperTypes="//structure/MultiplicityElement">
      <eStructuralFeatures xsi:type="ecore:EReference" name="operation" eType="//structure/Operation" transient="true" eOpposite="//structure/Operation/ownedParameter"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EDataType" name="UnlimitedNatural" instanceClassName="kermeta::standard::UnlimitedNatural"/>
  </eSubpackages>
</ecore:EPackage>
