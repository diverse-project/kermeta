// Type checking of kermeta weavinf

package weavinginvalidtest7;

using kermeta::standard
//------------------------------
// Base classes
//------------------------------
class A {
	operation amethod() : A[0..*] is abstract
}



//------------------------------
// Aspect classes
//------------------------------

@aspect "true"
class A {
	operation amethod() : A is do
		result := OrderedSet<A>.new
	end

}

