package mt_property_order;


require kermeta
using kermeta::standard

// ---------------------
modeltype mStandard {
  class A {
    reference aB : B[0..*]#aA
  }

  class B {
  	reference aA : A#aB
  }
}
class NeedsMStandard<MT : mStandard> { }

// ---------------------
modeltype mSet {
  class A {
    reference aB : set B[0..*]#aA
  }

  class B {
  	reference aA : A#aB
  }
}
class NeedsMSet<MT : mSet> { }

// ---------------------
modeltype mOSet {
  class A {
    reference aB : oset B[0..*]#aA
  }

  class B {
  	reference aA : A#aB
  }
}
class NeedsMOSet<MT : mOSet> { }


// ---------------------
modeltype mSeq {
  class A {
    reference aB : seq B[0..*]#aA
  }

  class B {
  	reference aA : A#aB
  }
}
class NeedsMSeq<MT : mSeq> { }

// ---------------------
modeltype mBag {
  class A {
    reference aB : bag B[0..*]#aA
  }

  class B {
  	reference aA : A#aB
  }
}
class NeedsMBag<MT : mBag> { }
