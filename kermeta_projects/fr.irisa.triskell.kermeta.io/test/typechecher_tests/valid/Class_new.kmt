// Class.new operation

package tctest;

class A {}

class B inherits A{}

class Test {

	operation test1() : Void is do
		A.new
	end
	
	operation test2() : Void is do
		var a : A
		a := B.new
	end
	
	operation test3() : Void is do
		var a : A
		a := A.new
	end
	
	operation test4() : Void is do
		var a : A
		var b : B
		a := B.new
		b ?= a
	end

}

