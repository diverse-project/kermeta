package mt_att_ref;


require kermeta
using kermeta::standard

package mRef {
  class A {
    reference aB : B#aA
  }

  class B {
  	reference aA : A#aB
  }
}
modeltype mRefT { mRef::A, mRef::B }
class NeedsMRef<MT : mRefT> { }


package mAtt {
  class A {
    attribute aB : B#aA
  }

  class B {
  	reference aA : A#aB
  }
}
modeltype mAttT { mAtt::A, mAtt::B }
class NeedsMAtt<MT : mAttT> { }

  