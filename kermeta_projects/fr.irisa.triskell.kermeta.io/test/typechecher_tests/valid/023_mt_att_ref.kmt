@mainClass "fsm::Main"
@mainOperation "main"


package mt_att_ref;


require kermeta
using kermeta::standard

modeltype mRef {
  class A {
    reference aB : B#aA
  }

  class B {
  	reference aA : A#aB
  }
}
class NeedsMRef<MT : mRef> { }


modeltype mAtt {
  class A {
    attribute aB : B#aA
  }

  class B {
  	reference aA : A#aB
  }
}
class NeedsMAtt<MT : mAtt> { }

  