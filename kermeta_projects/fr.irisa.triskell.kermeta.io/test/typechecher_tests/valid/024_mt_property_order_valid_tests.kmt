package mt_property_order;


require kermeta
require "024_mt_property_order.kmt"
using kermeta::standard

class Tester{
	operation testNeedsMStandard() : Void is do
		NeedsMStandard<mStandard>.new
		//NeedsMStandard<mSet>.new	// invalid see invalid testcase
		NeedsMStandard<mOSet>.new
		//NeedsMStandard<mSeq>.new	// invalid see invalid testcase
		//NeedsMStandard<mBag>.new	// invalid see invalid testcase
	end
	operation testNeedsMSet() : Void is do		
		NeedsMSet<mStandard>.new	// ordered set should be a valid non ordered set  (it inherits from it)
		NeedsMSet<mSet>.new
		NeedsMSet<mOSet>.new	// ordered set should be a valid non ordered set  (it inherits from it)
		//NeedsMSet<mSeq>.new	// invalid see invalid testcase
		//NeedsMSet<mBag>.new	// invalid see invalid testcase
	end
	
	
	operation testNeedsMOSet() : Void is do		
		NeedsMOSet<mStandard>.new
		//NeedsMOSet<mSet>.new	// invalid see invalid testcase
		NeedsMOSet<mOSet>.new
		//NeedsMOSet<mSeq>.new	// invalid see invalid testcase
		//NeedsMOSet<mBag>.new	// invalid see invalid testcase
	end
	
	operation testNeedsMSeq() : Void is do		
		//NeedsMSeq<mStandard>.new	// invalid see invalid testcase
		//NeedsMSeq<mSet>.new	// invalid see invalid testcase
		//NeedsMSeq<mOSet>.new	// invalid see invalid testcase
		NeedsMSeq<mSeq>.new
		//NeedsMSeq<mBag>.new	// invalid see invalid testcase
	end
	
	operation testNeedsMBag() : Void is do		
		//NeedsMBag<mStandard>.new	// invalid see invalid testcase
		//NeedsMBag<mSet>.new	// invalid see invalid testcase
		//NeedsMBag<mOSet>.new	// invalid see invalid testcase
		NeedsMBag<mSeq>.new	// a sequence should be a valid Bag (it inherits from it)
		NeedsMBag<mBag>.new
	end

}
  