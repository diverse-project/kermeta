@mainClass "root_package::Main"
@mainOperation "main"


package root_package;


require kermeta

using kermeta::standard

class Main
{
	operation eachOwnedElement(func0 : <Object -> Object> , func1 : <Object -> Object>) : Void is do
		self.ownedTags.each{ o |
			func0(o)
			func1(o)
		}
	end
}