package kermeta::language::structure;

require kermeta

using kermeta::standard

aspect class Constraint {
	
	/**
	 * Not used for the moment
	 */
	operation isPre(c : Constraint) : Boolean is do 
		result := (self.stereotype==ConstraintType.~pre)
	end

	/**
	 * Not used for the moment
	 */
	operation isPost(c : Constraint) : Boolean is do 
		result := (self.stereotype==ConstraintType.~post)
	end

}