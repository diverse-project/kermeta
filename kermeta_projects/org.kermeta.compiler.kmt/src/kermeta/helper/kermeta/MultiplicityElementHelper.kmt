package kermeta::language::structure;

require kermeta

require "TagHelper.kmt"

using kermeta::standard
using kermeta::utils

aspect class MultiplicityElement {

	operation getFinalName() : String is do
		result := self.name
		if self.getEMFRenameAs()!="" then
			result := self.getEMFRenameAs()
		end
	end
	
}