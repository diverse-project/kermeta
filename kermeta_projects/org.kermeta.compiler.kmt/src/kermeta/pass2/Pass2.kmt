package kermeta::language::structure;


require kermeta
require "SetType.kmt"
require "CreateBehavior.kmt"
require "../common/containment_traversable.kmt"

aspect class Object {
	operation applyPass2() is do
		self.setType()
		self.createBehavior()
		// Compile the owned elements
		self.eachOwnedElement{ p | p.applyPass2() }
	end
}