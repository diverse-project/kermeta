@mainClass "kermeta::compiler::test::Main"
@mainOperation "main"


package kermeta::compiler::test;


require kermeta
require "VoidLiteralTest.kmt"
require "VoidTypeTest.kmt"
require "TypeReferenceTest.kmt"
require "TypeLiteralTest.kmt"
require "StringLiteralTest.kmt"
require "SelfExpressionTest.kmt"
require "VariableDeclTest.kmt"
require "ClassTest.kmt"
require "AssignmentTest.kmt"
require "BlockTest.kmt"
require "BooleanLiteralTest.kmt"
require "IntegerLiteralTest.kmt"
require "RescueTest.kmt"
require "JavaStaticCallTest.kmt"

using kermeta::kunit

class Main inherits TestSuite {

	operation main() : Void is do
		addAllTestCasesFrom(VoidLiteralTest)
		addAllTestCasesFrom(VoidTypeTest)
		addAllTestCasesFrom(TypeReferenceTest)
		addAllTestCasesFrom(TypeLiteralTest)
		addAllTestCasesFrom(StringLiteralTest)	
		addAllTestCasesFrom(SelfExpressionTest)		
		addAllTestCasesFrom(VariableDeclTest)
		addAllTestCasesFrom(ClassTest)
		addAllTestCasesFrom(AssignmentTest)
		addAllTestCasesFrom(BlockTest)
		addAllTestCasesFrom(BooleanLiteralTest)
		addAllTestCasesFrom(IntegerLiteralTest)
		addAllTestCasesFrom(RescueTest)
		addAllTestCasesFrom(JavaStaticCallTest)
		var runner : TestRunner init TestRunner.new
		runner.runTest(self)
		runner.printTestResult()
	end

}