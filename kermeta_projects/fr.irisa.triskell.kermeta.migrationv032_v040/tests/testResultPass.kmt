@mainClass "root_package::Main"
@mainOperation "main"


package root_package;


require kermeta

using kermeta::standard
using kermeta::exceptions

class Main
{
    operation main() : Void is do 
        truc("bidule")
    end
    
    
    method truc(param : String)  : String is
     do   	
        stdio.writeln("test")
        result := param
     end
     post lapost is do result == "bidule" end
}