@uri "http://SimpleClass"
package SimpleClass;

require "kermeta"
class Classifier
{
	attribute name : PrimitiveTypes::String[1..1]

}
class Class inherits Classifier
{
	attribute is_persistent : PrimitiveTypes::Boolean[1..1]

	reference parent : Class

	attribute attrs : set Attribute[0..*]

}
class PrimitiveDataType inherits Classifier
{
}
class Association
{
	attribute name : PrimitiveTypes::String[1..1]

	reference src : Class[1..1]

	reference dest : Class[1..1]

}
class Attribute
{
	attribute name : PrimitiveTypes::String[1..1]

	attribute is_primary : PrimitiveTypes::Boolean[1..1]

	reference type : Classifier[1..1]

}
@uri "http://PrimitiveTypes"
package PrimitiveTypes;

require "kermeta"
alias String : kermeta::standard::String;
alias Boolean : kermeta::language::structure::Object;
