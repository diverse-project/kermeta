package mmwithderivedproperties;


require kermeta

using kermeta::standard

// kermeta version of the metamodel
class AWithDerivedPropInKermeta {

	attribute realAttributeB : BWithDerivedPropInKermeta
	
	property aB : BWithDerivedPropInKermeta 
		getter is do
			result := realAttributeB
		end
		setter is do
			realAttributeB := value
		end
}
class BWithDerivedPropInKermeta {
}

class AWithDerivedRefInEcore {
	attribute aB : BWithDerivedRefInEcore
}
class BWithDerivedRefInEcore {
}

