@mainClass "resultPass::Main"
@mainOperation "main"


package resultPass;


require kermeta

using kermeta::standard
using kermeta::exceptions
using kermeta::kunit

class Main inherits TestCase
{

	operation main() : Void is do
    	var tr : TestRunner init TestRunner.new
		tr.run(Main)
		tr.printTestResult
    end		

    operation test0() : Void is do 
        truc("bidule")
        assert(true)
       	rescue (err : ConstraintViolatedPost)
       		stdio.writeln(err.toString)
       		stdio.writeln(err.message)
       		assert(false) 
    end
    
    
    operation truc(param : String)  : String
    post lapost is do result == "bidule" end
    is
     do   	
        stdio.writeln("test")
        result := param
     end
     
}