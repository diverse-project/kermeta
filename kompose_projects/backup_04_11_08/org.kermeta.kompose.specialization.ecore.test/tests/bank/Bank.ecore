<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="Bank">
  <eClassifiers xsi:type="ecore:EClass" name="BankUser"/>
  <eClassifiers xsi:type="ecore:EClass" name="Controller">
    <eOperations name="transfer">
      <eParameters name="from" eType="//Account"/>
      <eParameters name="to" eType="//Account"/>
      <eParameters name="amount" eType="//Money"/>
    </eOperations>
    <eOperations name="deposit">
      <eParameters name="account" eType="//Account"/>
      <eParameters name="amount" eType="//Money"/>
    </eOperations>
    <eOperations name="withdraw">
      <eParameters name="account" eType="//Account"/>
      <eParameters name="amount" eType="//Money"/>
    </eOperations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="user" upperBound="-1" eType="//BankUser"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="account" upperBound="-1" eType="//Account"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Account">
    <eOperations name="deposit">
      <eParameters name="amount" eType="//Money"/>
    </eOperations>
    <eOperations name="withdraw">
      <eParameters name="amount" eType="//Money"/>
    </eOperations>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EDataType" name="Money" instanceClassName=""/>
</ecore:EPackage>
