package MARTE::MARTE_DesignModel::HRM::HwGeneral;
require "../../../../../model/ecore/MARTE.profile.ecore"
require kermeta
using kermeta::standard



aspect class HwResourceService
{
	/**
	* [20] power consumption is greater than dissipation.
	* reference MARTE 09-11-02 p271
	*/
	inv powerConsumption is do
		self.consumption.~value > self.dissipation.~value
	end	
}