<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
	<extension point="org.eclipse.help.toc">
		<toc primary="true" file="help/toc.xml"></toc>
	</extension>
	<extension
         point="org.eclipse.ui.newWizards">
      <category
            id="fr.irisa.triskell.kermeta.samples.category"
            name="Kermeta samples"
            parentCategory="org.eclipse.ui.Examples"/>
      <wizard
            canFinishEarly="true"
            category="org.eclipse.ui.Examples/fr.irisa.triskell.kermeta.samples.category"
            class="org.kermeta.tutorial.aspects.documentation.wizards.TutorialAspectsExampleWizard"
            finalPerspective="kermetaPerspective"
            hasPages="false"
            icon="icons/newkermetafile.png"
            id="org.kermeta.tutorial.aspects.wizard"
            name="Kermeta tutorial on Aspect Programming"
            preferredPerspectives="org.eclipse.ui.resourcePerspective"/>
   </extension>
</plugin>
