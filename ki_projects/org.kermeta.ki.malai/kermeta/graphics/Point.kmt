package kermeta::ki::malai::graphics;


require kermeta

using kermeta::standard

class Point
{
	attribute px : Real
	
	attribute py : Real
	
	
	operation initialise(x : Real, y : Real) : Void is do
		px := x
		py := y
	end
}
