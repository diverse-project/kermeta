package ecore;
require kermeta
using kermeta::standard
using kermeta::utils
class Pass2TemplatePC{
operation generate(classQualifiedName:String):String is do
var _res: StringBuffer init StringBuffer.new
_res.append("\t\tvar varName : String init ctx.cache.getValue(self)\n\t\tctx.res.append(\"$\"+varName+\": ")
_res.append(classQualifiedName)
_res.append("(\")\n\t\tctx.res.append(\"this == $\"+varName+\"Decl\")\n\t\t")
result := _res.toString
end
}
