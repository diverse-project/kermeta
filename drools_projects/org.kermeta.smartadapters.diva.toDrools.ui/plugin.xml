            nameFilter="*.smART"
            objectClass="org.eclipse.core.resources.IFile">
         <menu
               icon="icons/DivaModelFile.gif"
               id="smartadapters.diva.menu"
               label="SmartAdapters4DiVA"
               path="additions">
            <separator
                  name="group1">
            </separator>
         </menu>
         <action
               class="org.kermeta.smartadapters.diva.toDrools.ui.CompilerUI"
               icon="icons/DivaModelFile.gif"
