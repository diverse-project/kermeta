package my_package;

require kermeta

class A {
   attribute b : B[0..*]#a
}
class B {
   reference a : A[1..1]#b
}

class Usage {
   operation main() is do
      var a1 : A init A.new
      var b1 : B init B.new
      a1.b.add(b1)
      var a2 : A
      a2 := b1.a
   end
}