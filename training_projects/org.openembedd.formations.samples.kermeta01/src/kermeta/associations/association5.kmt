package my_package;

require kermeta

class A {
   reference b : B#a  // A contains a B
}
class B {
   attribute a : A#b
}

class Usage {
   operation main() is do
      var a1 : A init A.new
      var b1 : B init B.new
      a1.b := b1
      var b2 : B
      b2 := b2.a.b
      var a2 : A
      a2 := b1.a
   end
}