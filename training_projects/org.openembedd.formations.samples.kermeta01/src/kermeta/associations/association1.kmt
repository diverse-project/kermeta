package my_package;

require kermeta

class A {
   attribute b : B
}
class B {}

class Usage {
   operation main() is do
      var a1 : A init A.new
      var b1 : B init B.new
      a1.b := b1
      var b2 : B
      b2 := a1.b
   end
}