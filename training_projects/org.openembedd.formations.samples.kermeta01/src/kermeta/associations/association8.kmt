package my_package;

require kermeta

class A {
   reference sub : A[0..*]#up
   reference up : A#sub
}

class Usage {
   operation main() is do
      var a1 : A init A.new
      var a2 : A init A.new
      a1.sub.add(a2)
      var a3 : A
      a3 := a2.up
   end
}