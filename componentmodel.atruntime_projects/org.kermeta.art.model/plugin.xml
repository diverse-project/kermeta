<plugin>

  <extension point="org.eclipse.emf.ecore.generated_package">
    <package 
       uri = "http://art" 
       class = "art.ArtPackage"
       genModel = "model/metamodel@runtime.genmodel" /> 
  </extension>

  <extension point="org.eclipse.emf.ecore.generated_package">
    <package 
       uri = "http://art/instance" 
       class = "art.instance.InstancePackage"
       genModel = "model/metamodel@runtime.genmodel" /> 
  </extension>

  <extension point="org.eclipse.emf.ecore.generated_package">
    <package 
       uri = "http://art/type" 
       class = "art.type.TypePackage"
       genModel = "model/metamodel@runtime.genmodel" /> 
  </extension>

</plugin>
