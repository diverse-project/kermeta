<?xml version="1.0" encoding="ISO-8859-15"?>
<sca:composite xmlns:sca="http://www.osoa.org/xmlns/sca/1.0" name="tutorial_type_diagram" targetNamespace="http://eclipse.org/tutorial/model/tutorial_type_diagram">
  <sca:component constrainingType="ClientFactory" name="eu.ict_diva.tutorial.osgi.type.Client">
    <sca:implementation.java class="ClientImpl"/>
    <sca:reference multiplicity="0..n" name="defaultHelloWorld">
      <sca:interface.java interface="tutorial.diva.HelloWorld"/>
    </sca:reference>
    <sca:reference multiplicity="1..1" name="otherHelloWorlds">
      <sca:interface.java interface="tutorial.diva.HelloWorld"/>
    </sca:reference>
  </sca:component>
  <sca:component constrainingType="ServerFactory" name="eu.ict_diva.tutorial.osgi.type.Server">
    <sca:implementation.java class="HelloWorldImpl"/>
    <sca:service name="helloWorld">
      <sca:interface.java interface="tutorial.diva.HelloWorld"/>
    </sca:service>
    <sca:reference name="voiceHelloWorld">
      <sca:interface.java interface="tutorial.diva.VoiceHelloWorld"/>
    </sca:reference>
  </sca:component>
  <sca:component constrainingType="VoiceServerFactory" name="eu.ict_diva.tutorial.osgi.type.VoiceServer">
    <sca:implementation.java class="VoiceHelloWorldImpl"/>
    <sca:service name="voiceHelloWorld">
      <sca:interface.java interface="tutorial.diva.VoiceHelloWorld"/>
    </sca:service>
  </sca:component>
</sca:composite>
