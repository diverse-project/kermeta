      id = "org.eclipse.emf.ecore.Wizard.category.ID"
      name="%_UI_Wizard_category">
    </category>
    <wizard
      id = "art.presentation.ArtModelWizardID"
      name = "%_UI_ArtModelWizard_label"
      class = "art.presentation.ArtModelWizard"
      category = "org.eclipse.emf.ecore.Wizard.category.ID"
      icon = "icons/full/obj16/ArtModelFile.gif">
      <description>%_UI_ArtModelWizard_description</description>
      <selection class = "org.eclipse.core.resources.IResource" />
    </wizard>
  </extension>

  <extension point = "org.eclipse.ui.editors">
    <editor
        id = "art.presentation.ArtEditorID"
