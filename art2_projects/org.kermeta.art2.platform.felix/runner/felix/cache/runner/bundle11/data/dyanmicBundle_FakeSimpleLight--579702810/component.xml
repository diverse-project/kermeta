<blueprint xsi:schemaLocation="http://www.osgi.org/xmlns/blueprint/v1.0.0 http://www.osgi.org/xmlns/blueprint/v1.0.0/blueprint.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0">
      <reference interface="javax.jms.ConnectionFactory" id="cf"></reference>
      <bean factory-method="createFakeSimpleLightPORTon" class="org.entimid.fakeStuff.devices.FakeSimpleLightFactory" id="on"></bean><service ref="on" interface="org.entimid.framework.SimpleActionService">
                <service-properties>
                  <entry key="artPortName" value="on"></entry>
                  <entry key="artComponentName" value="FakeSimpleLight--579702810"></entry>
                </service-properties>
              </service><bean factory-method="createFakeSimpleLightPORTPROXYon" class="org.entimid.fakeStuff.devices.FakeSimpleLightFactory" id="onproxy">
                <property name="delegate"><ref component-id="on"></ref></property>
              </bean><bean destroy-method="stop" factory-method="createConsumer" class="org.kermeta.art2.runtime.proxy.Factory" init-method="init" id="onproxyConsumer">
                <property value="FakeSimpleLight--579702810.on" name="uri"></property>
                <property ref="cf" name="cf"></property>
                <property value="queue" name="hubType"></property>
                <property name="delegate"><ref component-id="onproxy"></ref></property>
              </bean><bean factory-method="createFakeSimpleLightPORToff" class="org.entimid.fakeStuff.devices.FakeSimpleLightFactory" id="off"></bean><service ref="off" interface="org.entimid.framework.SimpleActionService">
                <service-properties>
                  <entry key="artPortName" value="off"></entry>
                  <entry key="artComponentName" value="FakeSimpleLight--579702810"></entry>
                </service-properties>
              </service><bean factory-method="createFakeSimpleLightPORTPROXYoff" class="org.entimid.fakeStuff.devices.FakeSimpleLightFactory" id="offproxy">
                <property name="delegate"><ref component-id="off"></ref></property>
              </bean><bean destroy-method="stop" factory-method="createConsumer" class="org.kermeta.art2.runtime.proxy.Factory" init-method="init" id="offproxyConsumer">
                <property value="FakeSimpleLight--579702810.off" name="uri"></property>
                <property ref="cf" name="cf"></property>
                <property value="queue" name="hubType"></property>
                <property name="delegate"><ref component-id="offproxy"></ref></property>
              </bean>
      <!-- create the bean -->
      <bean destroy-method="stop" factory-method="createFakeSimpleLight" class="org.entimid.fakeStuff.devices.FakeSimpleLightFactory" init-method="init" id="FakeSimpleLight--579702810">

        <property name="dictionary">
          <props>
            <prop key="art.name">FakeSimpleLight--579702810</prop>
          </props>
        </property>

        <property name="hostedPorts">
          <map>
            <entry key="on">
                      <ref component-id="on"></ref>
                    </entry><entry key="off">
                      <ref component-id="off"></ref>
                    </entry>
          </map>
        </property>
        <property name="neededPorts">
          <map>
            
          </map>
        </property>
      </bean>
    </blueprint>