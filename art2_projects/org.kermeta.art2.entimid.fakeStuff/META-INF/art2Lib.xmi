<?xml version="1.0" encoding="ASCII"?>
<art2:ContainerRoot xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:art2="http://art/2.0">
  <componentTypes name="FakeSimpleSwitch" factoryBean="org.entimid.fakeStuff.devices.FakeSimpleSwitchFactory">
    <required name="on" ref="//@portTypes.0"/>
    <required name="off" ref="//@portTypes.0"/>
  </componentTypes>
  <componentTypes name="FakeSimpleLight" factoryBean="org.entimid.fakeStuff.devices.FakeSimpleLightFactory">
    <provided name="on" ref="//@portTypes.0">
      <mappings beanMethodName="lightUp" serviceMethodName="process"/>
    </provided>
    <provided name="off" ref="//@portTypes.0">
      <mappings beanMethodName="lightOff" serviceMethodName="process"/>
    </provided>
  </componentTypes>
  <dataTypes name="void"/>
  <portTypes xsi:type="art2:ServicePortType" name="org.entimid.framework.SimpleActionService">
    <operations name="process" returnType="//@dataTypes.0"/>
  </portTypes>
  <librariy subComponentTypes="//@componentTypes.0 //@componentTypes.1"/>
</art2:ContainerRoot>
