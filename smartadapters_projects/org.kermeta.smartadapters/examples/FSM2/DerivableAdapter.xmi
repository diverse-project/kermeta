<?xml version="1.0" encoding="ASCII"?>
<extension:DerivableAdapter xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:adaptations="platform:/resource/SmartAdapters/metamodel/specific/FSM_AOM_Framework.ecore#/org/smartadapters/core/adaptations" xmlns:extension="platform:/resource/SmartAdapters/metamodel/generic/genericSmartAdaptersBase.ecore#/org/smartadapters/extension" xsi:schemaLocation="platform:/resource/SmartAdapters/metamodel/specific/FSM_AOM_Framework.ecore#/org/smartadapters/core/adaptations ../../metamodel/specific/FSM_AOM_Framework.ecore#//smartadapters/core/adaptations platform:/resource/SmartAdapters/metamodel/generic/genericSmartAdaptersBase.ecore#/org/smartadapters/extension ../../metamodel/specific/FSM_AOM_Framework.ecore#//smartadapters/extension">
  <aspect>
    <template>
      <pattern>
        <content href="templateFsm.xmi#/"/>
        <content href="templateFsm.xmi#//@states.0"/>
        <content href="templateFsm.xmi#//@states.1"/>
        <content href="templateFsm.xmi#//@transitions.0"/>
        <roles name="anyFSM">
          <player href="templateFsm.xmi#/"/>
        </roles>
        <roles name="anyState">
          <player href="templateFsm.xmi#//@states.0"/>
        </roles>
        <roles name="anotherState">
          <player href="templateFsm.xmi#//@states.1"/>
        </roles>
        <roles name="anyToAnotherTransition">
          <player href="templateFsm.xmi#//@transitions.0"/>
        </roles>
      </pattern>
    </template>
  </aspect>
  <alternatives>
    <xor xsi:type="extension:EntityConjunction" name="complex variant">
      <and xsi:type="adaptations:CloneState">
        <StateToClone href="templateFsm.xmi#//@states.0"/>
        <clonedState name="clonedAny"/>
      </and>
      <and xsi:type="adaptations:CloneState" name="">
        <StateToClone href="templateFsm.xmi#//@states.1"/>
        <clonedState name="clonedAnother"/>
      </and>
      <and xsi:type="adaptations:CreateTransition" isUnique="non_unique">
        <newTransition/>
      </and>
      <and xsi:type="adaptations:SetTransition" TransitionToSet="//@alternatives.0/@xor.0/@and.2/@newTransition" refsource="//@alternatives.0/@xor.0/@and.1/@clonedState" reftarget="//@alternatives.0/@xor.0/@and.0/@clonedState"/>
      <and xsi:type="adaptations:SetFsm" refstates="//@alternatives.0/@xor.0/@and.0/@clonedState //@alternatives.0/@xor.0/@and.1/@clonedState" reftransitions="//@alternatives.0/@xor.0/@and.2/@newTransition">
        <FsmToSet href="templateFsm.xmi#/"/>
      </and>
      <and xsi:type="adaptations:UnsetFsm" name="" isOptional="true">
        <FsmToUnset href="templateFsm.xmi#/"/>
        <refstates href="templateFsm.xmi#//@states.0"/>
        <refstates href="templateFsm.xmi#//@states.1"/>
        <reftransitions href="templateFsm.xmi#//@transitions.0"/>
      </and>
    </xor>
    <xor xsi:type="extension:EntityConjunction" name="simple variant">
      <and xsi:type="adaptations:SetTransition" name="">
        <TransitionToSet href="templateFsm.xmi#//@transitions.0"/>
        <refsource href="templateFsm.xmi#//@states.1"/>
        <reftarget href="templateFsm.xmi#//@states.0"/>
      </and>
    </xor>
  </alternatives>
</extension:DerivableAdapter>
