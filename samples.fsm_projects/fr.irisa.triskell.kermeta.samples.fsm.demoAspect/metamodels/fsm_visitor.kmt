package fsm;

require kermeta
require "fsm_Helper.kmt"
using fsm
using kermeta::standard
using kermeta::persistence
using kermeta::exceptions

aspect class FSM
{
	
	operation accept(v: Visitor) : Void is do
				v.visitFSM(self)
	end
	
}

aspect class State {
	operation accept(v: Visitor) : Void is do
		v.visitState(self)		
	end
}

aspect class Transition 
{	
	operation accept(v: Visitor) : Void is do
		v.visitTransition(self)
	end
}

class Visitor{

	operation visitFSM(fsm: FSM) : Void is abstract
	operation visitState(state: State) : Void is abstract
	operation visitTransition(trans: Transition) : Void is abstract


}
