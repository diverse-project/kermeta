   <extension point="org.eclipse.ui.navigator.navigatorContent" id="navigator-content">
      <?gmfgen generated="true"?>
      <navigatorContent
            id="fr.irisa.triskell.kermeta.samples.fsm.gmf.diagram.resourceContent" 
            name="%navigatorContentName" 
            priority="normal" 
            contentProvider="fsm.diagram.navigator.FsmNavigatorContentProvider" 
            labelProvider="fsm.diagram.navigator.FsmNavigatorLabelProvider"
            icon="icons/obj16/FsmDemoDiagramFile.gif"
            activeByDefault="true">
         <triggerPoints>
            <or>
	           <and>
    	          <instanceof value="org.eclipse.core.resources.IFile"/>
        	      <test property="org.eclipse.core.resources.extension" value="fsmdemo_diagram"/>
               </and>
               <instanceof value="fsm.diagram.navigator.FsmAbstractNavigatorItem"/>
            </or>
         </triggerPoints>
         <possibleChildren>
            <or>
         	   <instanceof value="fsm.diagram.navigator.FsmAbstractNavigatorItem"/>
            </or>
         </possibleChildren>
         <commonSorter 
               id="fr.irisa.triskell.kermeta.samples.fsm.gmf.diagram.navigatorSorter" 
               class="fsm.diagram.navigator.FsmNavigatorSorter">
            <parentExpression>
               <or>
	              <and>
    	             <instanceof value="org.eclipse.core.resources.IFile"/>
        	         <test property="org.eclipse.core.resources.extension" value="fsmdemo_diagram"/>
                  </and>
                  <instanceof value="fsm.diagram.navigator.FsmAbstractNavigatorItem"/>
               </or>
            </parentExpression>
         </commonSorter>
      </navigatorContent>
      <navigatorContent
