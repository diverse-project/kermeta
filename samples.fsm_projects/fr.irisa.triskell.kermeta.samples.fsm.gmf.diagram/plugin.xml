               </and>
               <instanceof value="fsm.diagram.navigator.FsmDomainNavigatorItem"/>
            </or>
         </triggerPoints>
         <possibleChildren>
            <instanceof value="fsm.diagram.navigator.FsmDomainNavigatorItem"/>
         </possibleChildren>
      </navigatorContent>
      <actionProvider
            id="fr.irisa.triskell.kermeta.samples.fsm.gmf.diagram.navigatorActionProvider"
            class="fsm.diagram.navigator.FsmNavigatorActionProvider">
         <enablement>
            <or>
