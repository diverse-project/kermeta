<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>
	<artifactId>kompren.slicer.texteditor</artifactId>
    <packaging>jar</packaging>
    <name>kompren.slicer.texteditor :: Kompren text editor</name>

    <parent>
        <groupId>org.kermeta.kompren</groupId>
        <artifactId>kompren.root</artifactId>
        <version>2.0.9-SNAPSHOT</version>
    </parent>

  <build>
    <sourceDirectory>src</sourceDirectory>
    
		<resources>
			<resource>
				<directory>.</directory>
				<includes>
					<include>plugin.properties</include>
					<include>plugin.xml</include>
				</includes>
			</resource>
			<resource>
				<directory>icons</directory>
				<targetPath>icons</targetPath>
			</resource>
		</resources>
		
    <plugins>
            <plugin>
		        <groupId>org.apache.maven.plugins</groupId>
		        <artifactId>maven-jar-plugin</artifactId>
		        <version>2.3.1</version>
		        <configuration>
		          <archive>
		            <manifestEntries>
			            <Bundle-SymbolicName>org.kermeta.${project.artifactId}</Bundle-SymbolicName>
                        <Bundle-Name>${project.name}</Bundle-Name>
                        <Bundle-Vendor>${bundle.vendor}</Bundle-Vendor>
						<Bundle-Version>${bundle.version}</Bundle-Version>
                        <Bundle-ClassPath>.</Bundle-ClassPath>
                        <Bundle-ManifestVersion>2</Bundle-ManifestVersion>
                        <Bundle-SymbolicName>org.kermeta.kompren.slicer.texteditor;singleton:=true</Bundle-SymbolicName>
                        <Bundle-Activator>org.kermeta.kompren.texteditor.Activator</Bundle-Activator>
						<Require-Bundle>
			org.eclipse.emf.workspace;bundle-version="1.5.1",
			org.eclipse.core.runtime;bundle-version="3.6.0",
			org.eclipse.osgi;bundle-version="3.6.2",
			org.eclipse.core.jobs;bundle-version="3.5.1",
			org.eclipse.equinox.preferences;bundle-version="3.3.0",
			org.eclipse.core.resources;bundle-version="3.6.1",
			org.eclipse.core.commands;bundle-version="3.6.0",
			org.eclipse.emf.edit;bundle-version="2.6.0",
			org.eclipse.emf.ecore;bundle-version="2.6.1",
			org.eclipse.ui;bundle-version="3.6.2",
			org.eclipse.swt;bundle-version="3.6.2",
			org.eclipse.jface;bundle-version="3.6.2",
			org.eclipse.ui.workbench;bundle-version="3.6.2",
			org.eclipse.ui.ide;bundle-version="3.6.2",
			org.eclipse.ui.editors;bundle-version="3.6.1",
			org.eclipse.core.filebuffers;bundle-version="3.5.100",
			org.eclipse.jface.text;bundle-version="3.6.1",
			org.eclipse.ui.views;bundle-version="3.5.1",
			org.eclipse.ui.workbench.texteditor;bundle-version="3.6.1",
			org.kermeta.utils.systemservices.api,
			org.kermeta.utils.systemservices.eclipse,
			org.kermeta.kompren.slicer.parser
                        </Require-Bundle>
                        <Export-Package>
        	org.kermeta.kompren.slicer.texteditor.wizards,
        	org.kermeta.kompren.texteditor,
        	org.kermeta.kompren.texteditor.editors
                        </Export-Package>
		            </manifestEntries>		            
		          </archive>
		        </configuration>
      		</plugin>
    </plugins>
  </build>
