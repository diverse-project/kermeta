    <modelVersion>4.0.0</modelVersion>
	<artifactId>kompren.slicer.texteditor</artifactId>
    <packaging>bundle</packaging>
    <name>kompren.slicer.texteditor :: Kompren text editor</name>

    <parent>
        <groupId>org.kermeta.kompren</groupId>
        <artifactId>kompren.root</artifactId>
        <version>2.0.0-SNAPSHOT</version>
    </parent>

  <build>
    <sourceDirectory>src</sourceDirectory>
    
		<resources>
			<resource>
				<directory>.</directory>
				<includes>
					<include>plugin.properties</include>
					<include>plugin.xml</include>
				</includes>
			</resource>
			<resource>
				<directory>icons</directory>
				<targetPath>icons</targetPath>
			</resource>
		</resources>
		
    <plugins>
         <!-- Creation of the bundle jar and its manifest -->
            <plugin>
                <groupId>org.apache.felix</groupId>
                <artifactId>maven-bundle-plugin</artifactId>
                <version>2.1.0</version>
                <extensions>true</extensions>
                <configuration>
                    <instructions>
                        <Bundle-SymbolicName>org.kermeta.kompren.slicer.texteditor; singleton=true</Bundle-SymbolicName>
                        <Bundle-Name>${project.name}</Bundle-Name>
                        <Bundle-Vendor>${bundle.vendor}</Bundle-Vendor>
                        <Bundle-ManifestVersion>2</Bundle-ManifestVersion>
                        <Export-Package>
                        	org2.kermeta.kompren.texteditor.*,
                        	org2.kermeta.kompren.texteditor.editors.*
                        </Export-Package>
                        <Import-Package>
                        	org.eclipse.emf.*,
                        	org.osgi.framework;version="[1.5,2)",
                        	*
                        </Import-Package>
                        <Bundle-ClassPath>.</Bundle-ClassPath>
                    </instructions>
                </configuration>
            </plugin>
    </plugins>
  </build>
