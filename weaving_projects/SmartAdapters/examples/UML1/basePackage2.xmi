<?xml version="1.0" encoding="ASCII"?>
<uml:Package xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:uml="platform:/resource/SmartAdapters/metamodel/domain/ClassDiagUMLPattern.ecore#/uml"
    xsi:schemaLocation="platform:/resource/SmartAdapters/metamodel/domain/ClassDiagUMLPattern.ecore#/uml ../../metamodel/domain/ClassDiagUMLPattern.ecore#//uml">
      <packagedElement xsi:type="uml:Class" name="Manager">
    <ownedAttribute name="m" type="//MgrId"/>
  </packagedElement>
  <packagedElement xsi:type="uml:Class" name="UserMgmt">
    <ownedOperation name="addUser">
      <ownedParameter name="u" type="//UID"/>
    </ownedOperation>
    <ownedOperation name="deleteUser">
      <ownedParameter name="u" type="//UID"/>
    </ownedOperation>
  </packagedElement>
  <packagedElement xsi:type="uml:Class" name="DataMgmt">
    <ownedOperation name="addData">
      <ownedParameter name="d" type="//DID"/>
    </ownedOperation>
    <ownedOperation name="deleteData">
      <ownedParameter name="d" type="//DID"/>
    </ownedOperation>
  </packagedElement>
  <packagedElement xsi:type="uml:Class" name="UserRepository"/>
  <packagedElement xsi:type="uml:Class" name="DataRepository"/>
  <packagedElement xsi:type="uml:Association" name="userAccess" memberEnd="//userAccess/userManager //userAccess/userMgmt">
    <ownedEnd name="userManager" type="//Manager" association="//userAccess">
      <upperValue xsi:type="uml:LiteralUnlimitedNatural" value="*"/>
    </ownedEnd>
    <ownedEnd name="userMgmt" type="//UserMgmt" association="//userAccess"/>
  </packagedElement>
  <packagedElement xsi:type="uml:Association" name="dataAccess" memberEnd="//dataAccess/dataManager //dataAccess/dataMgmt">
    <ownedEnd name="dataManager" type="//Manager" association="//dataAccess">
      <upperValue xsi:type="uml:LiteralUnlimitedNatural" value="*"/>
    </ownedEnd>
    <ownedEnd name="dataMgmt" type="//DataMgmt" association="//dataAccess"/>
  </packagedElement>
  <packagedElement xsi:type="uml:Association" name="userRepAccess" memberEnd="//userRepAccess/userMgmt //userRepAccess/rep">
    <ownedEnd name="userMgmt" type="//UserMgmt" association="//userRepAccess"/>
    <ownedEnd name="rep" type="//UserRepository" association="//userRepAccess"/>
  </packagedElement>
  <packagedElement xsi:type="uml:Association" name="dataRepAccess" memberEnd="//dataRepAccess/dataMgmt //dataRepAccess/rep">
    <ownedEnd name="dataMgmt" type="//DataMgmt" association="//dataRepAccess"/>
    <ownedEnd name="rep" type="//DataRepository" association="//dataRepAccess"/>
  </packagedElement>
  <packagedElement xsi:type="uml:DataType" name="MgrId"/>
  <packagedElement xsi:type="uml:DataType" name="UID"/>
  <packagedElement xsi:type="uml:DataType" name="DID"/>
</uml:Package>
