@mainClass "root_package::EcorePatternMetamodelBuilder"
@mainOperation "main"


package root_package;


require kermeta
require "platform:/resource/SmartAdapters/program/PatternBuilder/MMPatternBuilder.kmt"
 
using kermeta::standard
using org::smartadapters::utils

 
class EcorePatternMetamodelBuilder {

  operation main() is do 
      var sourceMetamodelUri : String init "platform:/resource/SmartAdapters/metamodel/domain/uml.ecore"
      var targetMetamodelUri : String init "platform:/resource/SmartAdapters/metamodel/domain/umlPattern.ecore"
      
      var builder : PatternBuilder init PatternBuilder.new
      builder.relax(sourceMetamodelUri, targetMetamodelUri) 
  end
}