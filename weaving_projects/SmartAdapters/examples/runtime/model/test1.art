<?xml version="1.0" encoding="ASCII"?>
<art:System xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:art="http://art" xmlns:instance="http://art/instance" xmlns:type="http://art/type">
  <root name="rootComp" type="//@types.2" state="ON">
    <subComponent xsi:type="instance:PrimitiveInstance" name="cComp" type="//@types.3" state="ON"/>
    <subComponent xsi:type="instance:PrimitiveInstance" name="converterComp" type="//@types.1" state="ON">
      <binding serverInstance="//@root/@subComponent.0" client="//@types.1/@port.1/@sub.0" server="//@types.3/@port.0/@sub.0"/>
    </subComponent>
    <subComponent xsi:type="instance:PrimitiveInstance" name="mainComp" type="//@types.0" state="ON">
      <binding serverInstance="//@root/@subComponent.1" client="//@types.0/@port.1/@sub.0" server="//@types.1/@port.0/@sub.0"/>
    </subComponent>
    <delegation serverInstance="//@root/@subComponent.2" source="//@types.0/@port.0/@sub.0" exported="//@types.2/@port.0/@sub.0"/>
  </root>
  <services xsi:type="type:FunctionalService" name="org.kermeta.fractal.example.Business"/>
  <services xsi:type="type:FunctionalService" name="org.kermeta.fractal.example.Converter"/>
  <services xsi:type="type:FunctionalService" name="org.kermeta.fractal.example.Main"/>
  <types xsi:type="type:PrimitiveType" name="main.converter.">
    <port name="main" isOptional="false">
      <sub name="main" service="//@services.2" role="server"/>
    </port>
    <port name="converter" isOptional="false">
      <sub name="converter" service="//@services.1"/>
    </port>
  </types>
  <types xsi:type="type:PrimitiveType" name="converter.business.">
    <port name="converter" isOptional="false">
      <sub name="converter" service="//@services.1" role="server"/>
    </port>
    <port name="business" isOptional="false">
      <sub name="business" service="//@services.0"/>
    </port>
  </types>
  <types xsi:type="type:CompositeType" name="main.">
    <port name="main" isOptional="false">
      <sub name="main" service="//@services.2" role="server"/>
    </port>
  </types>
  <types xsi:type="type:PrimitiveType" name="business.">
    <port name="business">
      <sub name="business" service="//@services.0" role="server"/>
    </port>
  </types>
</art:System>
