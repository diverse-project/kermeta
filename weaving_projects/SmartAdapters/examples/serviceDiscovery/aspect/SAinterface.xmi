  <root name="Service Discovery" type="//@types.0" state="ON">
    <subComponent xsi:type="instance:PrimitiveInstance" name="Policy" type="//@types.3" state="ON"/>
    <subComponent xsi:type="instance:PrimitiveInstance" name="Cache" type="//@types.1" state="ON"/>
