<?xml version="1.0" encoding="ASCII"?>
<core:Adapter xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:adaptations="platform:/resource/SmartAdapters/metamodel/specific/FSM_AOM_Framework.ecore#//smartadapters/core/adaptations" xmlns:core="platform:/resource/SmartAdapters/metamodel/specific/FSM_AOM_Framework.ecore#//smartadapters/core" xsi:schemaLocation="platform:/resource/SmartAdapters/metamodel/specific/FSM_AOM_Framework.ecore#//smartadapters/core/adaptations ../../metamodel/specific/FSM_AOM_Framework.ecore#//smartadapters/core/adaptations platform:/resource/SmartAdapters/metamodel/specific/FSM_AOM_Framework.ecore#//smartadapters/core ../../metamodel/specific/FSM_AOM_Framework.ecore#//smartadapters/core">
  <aspect>
    <template>
      <pattern>
        <content href="templateFsm.xmi#/"/>
        <content href="templateFsm.xmi#//@states.0"/>
        <content href="templateFsm.xmi#//@states.1"/>
        <content href="templateFsm.xmi#//@transitions.0"/>
        <roles name="anyRole">
          <player href="templateFsm.xmi#//@states.0"/>
        </roles>
        <roles name="anotherRole">
          <player href="templateFsm.xmi#//@states.1"/>
        </roles>
        <roles name="transitionRole">
          <player href="templateFsm.xmi#//@transitions.0"/>
        </roles>
        <roles name="fsmRole">
          <player href="templateFsm.xmi#/"/>
        </roles>
      </pattern>
      <featureIdentifier>name</featureIdentifier>
    </template>
  </aspect>
  <adapt xsi:type="adaptations:cloneState" name="">
    <newState href="templateFsm.xmi#//@states.0"/>
    <realState name="clonedState"/>
  </adapt>
  <adapt xsi:type="adaptations:setFsm" astates="//@adapt.0/@realState">
    <aFsm href="templateFsm.xmi#/"/>
  </adapt>
  <adapt xsi:type="adaptations:unsetFsm">
    <aFsm href="templateFsm.xmi#/"/>
    <atransitions href="templateFsm.xmi#//@transitions.0"/>
  </adapt>
</core:Adapter>
