<?xml version="1.0" encoding="ASCII"?>
<core:Adapter xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:adaptations="platform:/resource/SmartAdapters/metamodel/specific/FSM_AOM_Framework.ecore#/org/smartadapters/core/adaptations" xmlns:core="platform:/resource/SmartAdapters/metamodel/generic/genericSmartAdaptersBase.ecore#/org/smartadapters/core" xsi:schemaLocation="platform:/resource/SmartAdapters/metamodel/specific/FSM_AOM_Framework.ecore#/org/smartadapters/core/adaptations ../../metamodel/specific/FSM_AOM_Framework.ecore#//smartadapters/core/adaptations platform:/resource/SmartAdapters/metamodel/generic/genericSmartAdaptersBase.ecore#/org/smartadapters/core ../../metamodel/specific/FSM_AOM_Framework.ecore#//smartadapters/core" name="Reverse state machine (simple version)">
  <aspect>
