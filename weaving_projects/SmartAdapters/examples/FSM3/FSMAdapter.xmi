<?xml version="1.0" encoding="ASCII"?>
<core:FSMAdapter xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:adaptations="platform:/resource/SmartAdapters/metamodel/specific/FSM_AOM_Framework.ecore#//smartadapters/core/adaptations" xmlns:core="platform:/resource/SmartAdapters/metamodel/specific/FSM_AOM_Framework.ecore#//smartadapters/core" xsi:schemaLocation="platform:/resource/SmartAdapters/metamodel/specific/FSM_AOM_Framework.ecore#//smartadapters/core/adaptations ../../metamodel/specific/FSM_AOM_Framework.ecore#//smartadapters/core/adaptations platform:/resource/SmartAdapters/metamodel/specific/FSM_AOM_Framework.ecore#//smartadapters/core ../../metamodel/specific/FSM_AOM_Framework.ecore#//smartadapters/core">
  <aspect>
    <template>
      <pattern>
        <content href="FsmTemplate.xmi#/"/>
        <content href="FsmTemplate.xmi#//@transitions.0"/>
        <content href="FsmTemplate.xmi#//@states.0"/>
        <content href="FsmTemplate.xmi#//@states.1"/>
        <roles name="anyFSM">
          <player href="FsmTemplate.xmi#/"/>
        </roles>
        <roles name="anyState">
          <player href="FsmTemplate.xmi#//@states.0"/>
        </roles>
        <roles name="anotherState">
          <player href="FsmTemplate.xmi#//@states.1"/>
        </roles>
        <roles name="any->another">
          <player href="FsmTemplate.xmi#//@transitions.0"/>
        </roles>
      </pattern>
      <featureIdentifier>name</featureIdentifier>
    </template>
  </aspect>
  <adapt xsi:type="adaptations:cloneState" name="">
    <newState href="FsmTemplate.xmi#//@states.0"/>
    <realState name="clonedAny"/>
  </adapt>
  <adapt xsi:type="adaptations:cloneState">
    <newState href="FsmTemplate.xmi#//@states.1"/>
    <realState name="clonedAnother"/>
  </adapt>
  <adapt xsi:type="adaptations:createTransition" name="">
    <newTransition target="//@adapt.1/@realState" source="//@adapt.0/@realState"/>
  </adapt>
  <adapt xsi:type="adaptations:setFsm" astates="//@adapt.0/@realState //@adapt.1/@realState" atransitions="//@adapt.2/@newTransition">
    <aFsm href="FsmTemplate.xmi#/"/>
  </adapt>
</core:FSMAdapter>
