@mainClass "test_package::MyTestClass" 
@mainOperation "main"


package test_package;

require kermeta

require "701test_CloneTool_Package.main.kmt"
require "702test_CloneTool_Class.main.kmt"
require "703test_CloneTool_Operation.main.kmt"
require "704test_CloneTool_Parameter.main.kmt"
require "705test_CloneTool_Property.main.kmt"
require "706test_CloneTool_ValueSpecification.main.kmt"
require "707test_CloneTool_Association.main.kmt"

require "750test_CloneTool_StaticDiagram.main.kmt"

using tools_tests

class MyTestClass
{
    operation main() : Void is do 
	    var testRunner : kermeta::kunit::TestRunner init kermeta::kunit::TestRunner.new
		
		///////// tests on UML2 elements cloning //////////
		
		testRunner.run(ToolClonePackageTestCase)
		testRunner.printTestResult
		
		testRunner.run(ToolCloneClassTestCase)
		testRunner.printTestResult
		
		testRunner.run(ToolCloneOperationTestCase)
		testRunner.printTestResult
		
		testRunner.run(ToolCloneParameterTestCase)
		testRunner.printTestResult
		
		testRunner.run(ToolClonePropertyTestCase)
		testRunner.printTestResult
		
		testRunner.run(ToolCloneValueSpecificationTestCase)
		testRunner.printTestResult
		
		testRunner.run(ToolCloneAssociationTestCase)
		testRunner.printTestResult
		
		///////// tests on UML2 diagrams cloning //////////
		
		testRunner.run(ToolCloneStaticDiagramTestCase)
		testRunner.printTestResult
    end 
}