}
class Extend
{
	reference base_Generalization : uml::Generalization[1..1]

}
enumeration ContentKind
{
	Unspecified;
	Empty;
	Simple;
	Mixed;
	ElementOnly;
}
class EClass inherits EClassifier
{
	reference base_Class : uml::Class[1..1]

	reference base_Interface : uml::Interface[1..1]

	attribute className : ecore::EString

	attribute xmlContentKind : ContentKind

