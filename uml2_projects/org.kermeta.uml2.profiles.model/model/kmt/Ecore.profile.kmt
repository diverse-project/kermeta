	attribute packageName : ecore::EString

	attribute nsPrefix : ecore::EString

	attribute nsURI : ecore::EString

	attribute basePackage : ecore::EString

	attribute prefix : ecore::EString

}
abstract class ENamedElement
{
	attribute xmlName : ecore::EString

}
abstract class EClassifier inherits ENamedElement
{
