@uri "http://www.eclipse.org/uml2/schemas/Ecore/3"
package Ecore;

require "http://www.eclipse.org/emf/2002/Ecore"
require "platform:/resource/org.kermeta.uml2.profiles.model/model/ecore/uml.ecore"
require "kermeta"
class EPackage
{
