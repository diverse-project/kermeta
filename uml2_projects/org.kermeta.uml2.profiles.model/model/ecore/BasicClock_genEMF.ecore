<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="BasicClock"
    nsURI="http:///BasicClock.ecore" nsPrefix="BasicClock">
  <eClassifiers xsi:type="ecore:EEnum" name="Brand">
    <eLiterals name="Casio"/>
    <eLiterals name="Citizen" value="1"/>
    <eLiterals name="Rolex" value="2"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="AbstractWatch">
