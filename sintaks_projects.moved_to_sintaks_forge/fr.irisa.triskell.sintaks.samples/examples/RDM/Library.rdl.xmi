<?xml version="1.0" encoding="ASCII"?>
<rdm:RDModel xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:rdm="http://rdm">
  <requirementGroups name="Configuration">
    <number>
      <numbers>1</numbers>
      <numbers>0</numbers>
    </number>
    <requirements xsi:type="rdm:ObjectDeclarationInstance" name="beloved" type="book"/>
    <requirements xsi:type="rdm:ObjectDeclarationInstance" name="emma" type="book"/>
    <requirements xsi:type="rdm:ObjectDeclarationInstance" name="peter" type="customer"/>
    <requirements xsi:type="rdm:ObjectDeclarationInstance" name="jane" type="customer"/>
    <requirements xsi:type="rdm:ObjectDeclarationInstance" name="mary" type="librarian"/>
  </requirementGroups>
  <requirementGroups name="Registering a book">
    <number>
      <numbers>1</numbers>
    </number>
    <requirements xsi:type="rdm:RequirementBasicCompositeTemporalAfter">
      <condition xsi:type="rdm:ServiceActivationDid" name="register">
        <complements name="book">
          <quantifier xsi:type="rdm:QuantifierThe"/>
        </complements>
        <subject name="librarian">
          <quantifier xsi:type="rdm:QuantifierThe"/>
        </subject>
      </condition>
      <consequence xsi:type="rdm:ObservablePropertyBecomes">
        <reference name="book">
          <quantifier xsi:type="rdm:QuantifierThe"/>
        </reference>
        <value xsi:type="rdm:ObservableValueSimple" name="registered"/>
      </consequence>
    </requirements>
    <requirements xsi:type="rdm:RequirementBasicCompositeTemporalAfter">
      <condition xsi:type="rdm:ServiceActivationDid" name="register">
        <complements name="book">
          <quantifier xsi:type="rdm:QuantifierThe"/>
        </complements>
        <subject name="librarian">
          <quantifier xsi:type="rdm:QuantifierThe"/>
        </subject>
      </condition>
      <consequence xsi:type="rdm:ObservablePropertyIs">
        <reference name="book">
          <quantifier xsi:type="rdm:QuantifierThe"/>
        </reference>
        <value xsi:type="rdm:ObservableValueSimple" name="available"/>
      </consequence>
    </requirements>
  </requirementGroups>
  <requirementGroups name="Unregistering a book">
    <number>
      <numbers>1</numbers>
      <numbers>2</numbers>
    </number>
    <requirements xsi:type="rdm:RequirementBasicCompositeTemporalAfter">
      <condition xsi:type="rdm:ServiceActivationDid" name="unregister">
        <complements name="book">
          <quantifier xsi:type="rdm:QuantifierThe"/>
        </complements>
        <subject name="librarian">
          <quantifier xsi:type="rdm:QuantifierThe"/>
        </subject>
      </condition>
      <consequence xsi:type="rdm:ObservablePropertyBecomes">
        <reference name="book">
          <quantifier xsi:type="rdm:QuantifierThe"/>
        </reference>
        <value xsi:type="rdm:ObservableValueNot">
          <delegate xsi:type="rdm:ObservableValueSimple" name="available"/>
        </value>
      </consequence>
    </requirements>
    <requirements xsi:type="rdm:RequirementBasicCompositeTemporalAfter">
      <condition xsi:type="rdm:ServiceActivationDid" name="unregister">
        <complements name="book">
          <quantifier xsi:type="rdm:QuantifierThe"/>
        </complements>
        <subject name="librarian">
          <quantifier xsi:type="rdm:QuantifierThe"/>
        </subject>
      </condition>
      <consequence xsi:type="rdm:ObservablePropertyIs">
        <reference name="book">
          <quantifier xsi:type="rdm:QuantifierThe"/>
        </reference>
        <value xsi:type="rdm:ObservableValueNot">
          <delegate xsi:type="rdm:ObservableValueSimple" name="registered"/>
        </value>
      </consequence>
    </requirements>
  </requirementGroups>
  <requirementGroups name="Register a customer">
    <number>
      <numbers>1</numbers>
      <numbers>3</numbers>
    </number>
    <requirements xsi:type="rdm:RequirementBasicCompositeTemporalAfter">
      <condition xsi:type="rdm:ServiceActivationDid" name="subscribe">
        <complements name="customer">
          <quantifier xsi:type="rdm:QuantifierThe"/>
        </complements>
        <subject name="librarian">
          <quantifier xsi:type="rdm:QuantifierThe"/>
        </subject>
      </condition>
      <consequence xsi:type="rdm:ObservablePropertyBecomes">
        <reference name="customer">
          <quantifier xsi:type="rdm:QuantifierThe"/>
        </reference>
        <value xsi:type="rdm:ObservableValueSimple" name="registered"/>
      </consequence>
    </requirements>
  </requirementGroups>
  <requirementGroups name="Borrow a book">
    <number>
      <numbers>1</numbers>
      <numbers>4</numbers>
    </number>
    <requirements xsi:type="rdm:RequirementBasicCompositeTemporalBefore">
      <condition xsi:type="rdm:ServiceActivationCan" name="borrow">
        <complements name="book">
          <quantifier xsi:type="rdm:QuantifierThe"/>
        </complements>
        <subject name="customer">
          <quantifier xsi:type="rdm:QuantifierThe"/>
        </subject>
      </condition>
      <consequence xsi:type="rdm:ObservablePropertyMustBe">
        <reference name="customer">
          <quantifier xsi:type="rdm:QuantifierThe"/>
        </reference>
        <value xsi:type="rdm:ObservableValueSimple" name="registered"/>
      </consequence>
    </requirements>
    <requirements xsi:type="rdm:RequirementBasicCompositeTemporalAfter">
      <condition xsi:type="rdm:ServiceActivationDid" name="borrow">
        <complements name="book">
          <quantifier xsi:type="rdm:QuantifierThe"/>
        </complements>
        <subject name="customer">
          <quantifier xsi:type="rdm:QuantifierThe"/>
        </subject>
      </condition>
      <consequence xsi:type="rdm:ObservablePropertyBecomes">
        <reference name="book">
          <quantifier xsi:type="rdm:QuantifierThe"/>
        </reference>
        <value xsi:type="rdm:ObservableValueNot">
          <delegate xsi:type="rdm:ObservableValueSimple" name="available"/>
        </value>
      </consequence>
    </requirements>
    <requirements xsi:type="rdm:RequirementBasicCompositeTemporalAfter">
      <condition xsi:type="rdm:ServiceActivationDid" name="borrow">
        <complements name="book">
          <quantifier xsi:type="rdm:QuantifierThe"/>
        </complements>
        <subject name="customer">
          <quantifier xsi:type="rdm:QuantifierThe"/>
        </subject>
      </condition>
      <consequence xsi:type="rdm:ObservablePropertyIs">
        <reference name="book">
          <quantifier xsi:type="rdm:QuantifierThe"/>
          <associatedOwners name="customer">
            <quantifier xsi:type="rdm:QuantifierThe"/>
          </associatedOwners>
        </reference>
        <value xsi:type="rdm:ObservableValueSimple" name="borrowed"/>
      </consequence>
    </requirements>
    <requirements xsi:type="rdm:RequirementBasicCompositeTemporalBefore">
      <condition xsi:type="rdm:ServiceActivationCan" name="borrow">
        <complements name="book">
          <quantifier xsi:type="rdm:QuantifierThe"/>
        </complements>
        <subject name="customer">
          <quantifier xsi:type="rdm:QuantifierThe"/>
        </subject>
      </condition>
      <consequence xsi:type="rdm:ObservablePropertyMustBe">
        <reference name="book">
          <quantifier xsi:type="rdm:QuantifierThe"/>
        </reference>
        <value xsi:type="rdm:ObservableValueNot">
          <delegate xsi:type="rdm:ObservableValueSimple" name="damaged"/>
        </value>
      </consequence>
    </requirements>
  </requirementGroups>
  <requirementGroups name="Return a book">
    <number>
      <numbers>1</numbers>
      <numbers>5</numbers>
    </number>
    <requirements xsi:type="rdm:RequirementBasicCompositeTemporalAfter">
      <condition xsi:type="rdm:ServiceActivationDid" name="return">
        <complements name="book">
          <quantifier xsi:type="rdm:QuantifierThe"/>
        </complements>
        <subject name="customer">
          <quantifier xsi:type="rdm:QuantifierThe"/>
        </subject>
      </condition>
      <consequence xsi:type="rdm:ObservablePropertyBecomes">
        <reference name="book">
          <quantifier xsi:type="rdm:QuantifierThe"/>
          <associatedOwners name="customer">
            <quantifier xsi:type="rdm:QuantifierThe"/>
          </associatedOwners>
        </reference>
        <value xsi:type="rdm:ObservableValueNot">
          <delegate xsi:type="rdm:ObservableValueSimple" name="borrowed"/>
        </value>
      </consequence>
    </requirements>
    <requirements xsi:type="rdm:RequirementBasicCompositeTemporalAfter">
      <condition xsi:type="rdm:ServiceActivationDid" name="return">
        <complements name="book">
          <quantifier xsi:type="rdm:QuantifierThe"/>
        </complements>
        <subject name="customer">
          <quantifier xsi:type="rdm:QuantifierThe"/>
        </subject>
      </condition>
      <consequence xsi:type="rdm:ObservablePropertyIs">
        <reference name="book">
          <quantifier xsi:type="rdm:QuantifierThe"/>
        </reference>
        <value xsi:type="rdm:ObservableValueSimple" name="available"/>
      </consequence>
    </requirements>
    <requirements xsi:type="rdm:RequirementBasicCompositeTemporalAfter">
      <condition xsi:type="rdm:ServiceActivationDid" name="return">
        <complements name="book">
          <quantifier xsi:type="rdm:QuantifierThe"/>
        </complements>
        <subject name="customer">
          <quantifier xsi:type="rdm:QuantifierThe"/>
        </subject>
      </condition>
      <consequence xsi:type="rdm:ObservablePropertyIs">
        <reference name="book">
          <quantifier xsi:type="rdm:QuantifierThe"/>
        </reference>
        <value xsi:type="rdm:ObservableValueSimple" name="damaged"/>
      </consequence>
    </requirements>
  </requirementGroups>
  <requirementGroups name="Inventory">
    <number>
      <numbers>1</numbers>
      <numbers>6</numbers>
    </number>
    <requirements xsi:type="rdm:RequirementBasicCompositeTemporalAfter">
      <condition xsi:type="rdm:ServiceActivationDid" name="make an inventory">
        <subject name="librarian">
          <quantifier xsi:type="rdm:QuantifierThe"/>
        </subject>
      </condition>
      <consequence xsi:type="rdm:ObservablePropertyIs">
        <reference name="book">
          <quantifier xsi:type="rdm:QuantifierEach"/>
        </reference>
        <value xsi:type="rdm:ObservableValueNot">
          <delegate xsi:type="rdm:ObservableValueSimple" name="damaged"/>
        </value>
      </consequence>
    </requirements>
  </requirementGroups>
</rdm:RDModel>
