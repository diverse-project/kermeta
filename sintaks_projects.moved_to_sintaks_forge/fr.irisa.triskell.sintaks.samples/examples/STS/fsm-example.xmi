<?xml version="1.0" encoding="ASCII"?>
<sts:Root xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" xmlns:sts="http://sts">
  <fragments xsi:type="sts:Template" id="FSM">
    <metaclass href="../../MetaModels/FSM.ecore#//FSM"/>
    <rule xsi:type="sts:Sequence">
      <subRules xsi:type="sts:Iteration">
        <subRule xsi:type="sts:Sequence">
          <subRules xsi:type="sts:RuleRef"/>
        </subRule>
        <container xsi:type="ecore:EReference" href="../../MetaModels/FSM.ecore#//FSM/ownedState"/>
      </subRules>
      <subRules xsi:type="sts:Iteration">
        <subRule xsi:type="sts:Sequence">
          <subRules xsi:type="sts:RuleRef"/>
        </subRule>
        <container xsi:type="ecore:EReference" href="../../MetaModels/FSM.ecore#//FSM/transition"/>
      </subRules>
    </rule>
  </fragments>
  <fragments xsi:type="sts:Template" id="State">
    <metaclass href="../../MetaModels/FSM.ecore#//State"/>
    <rule xsi:type="sts:Sequence">
      <subRules xsi:type="sts:Terminal" id="state" terminal="state"/>
      <subRules xsi:type="sts:PrimitiveValue">
        <features xsi:type="ecore:EAttribute" href="../../MetaModels/FSM.ecore#//State/name"/>
      </subRules>
      <subRules xsi:type="sts:Terminal" id=";" terminal=";"/>
    </rule>
  </fragments>
  <fragments xsi:type="sts:Template" id="Transition">
    <metaclass href="../../MetaModels/FSM.ecore#//Transition"/>
    <rule xsi:type="sts:Sequence">
      <subRules xsi:type="sts:Terminal" id="transition" terminal="transition"/>
      <subRules xsi:type="sts:Terminal" id="from" terminal="from"/>
      <subRules xsi:type="sts:ObjectReference">
        <features xsi:type="ecore:EReference" href="../../MetaModels/FSM.ecore#//Transition/source"/>
        <identifier xsi:type="ecore:EAttribute" href="../../MetaModels/FSM.ecore#//State/name"/>
      </subRules>
      <subRules xsi:type="sts:Terminal" id="to" terminal="to"/>
      <subRules xsi:type="sts:ObjectReference">
        <features xsi:type="ecore:EReference" href="../../MetaModels/FSM.ecore#//Transition/target"/>
        <identifier xsi:type="ecore:EAttribute" href="../../MetaModels/FSM.ecore#//State/name"/>
      </subRules>
      <subRules xsi:type="sts:Terminal" id="input" terminal="input"/>
      <subRules xsi:type="sts:PrimitiveValue">
        <features xsi:type="ecore:EAttribute" href="../../MetaModels/FSM.ecore#//Transition/input"/>
      </subRules>
      <subRules xsi:type="sts:Terminal" id="output" terminal="output"/>
      <subRules xsi:type="sts:PrimitiveValue">
        <features xsi:type="ecore:EAttribute" href="../../MetaModels/FSM.ecore#//Transition/output"/>
      </subRules>
      <subRules xsi:type="sts:Terminal" id=";" terminal=";"/>
    </rule>
  </fragments>
</sts:Root>
