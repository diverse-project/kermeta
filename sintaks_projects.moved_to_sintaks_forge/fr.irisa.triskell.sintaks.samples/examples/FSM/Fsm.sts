<?xml version="1.0" encoding="ASCII"?>
<sts:Root xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" xmlns:sts="http://sts" start="//@rules.0">
  <rules xsi:type="sts:Template" id="FSM">
    <metaclass href="http://fsm#//FSM"/>
    <rule xsi:type="sts:Sequence">
      <subRules xsi:type="sts:Iteration" id="FSM.ownedState">
        <subRule xsi:type="sts:RuleRef" id="@ State" ref="//@rules.1"/>
        <container xsi:type="ecore:EReference" href="http://fsm#//FSM/ownedState"/>
      </subRules>
      <subRules xsi:type="sts:Iteration" id="FSM.transition">
        <subRule xsi:type="sts:RuleRef" id="@ Transition" ref="//@rules.2"/>
        <container xsi:type="ecore:EReference" href="http://fsm#//FSM/transition"/>
      </subRules>
    </rule>
  </rules>
  <rules xsi:type="sts:Template" id="State">
    <metaclass href="http://fsm#//State"/>
    <rule xsi:type="sts:Sequence">
      <subRules xsi:type="sts:Terminal" id="state" terminal="state"/>
      <subRules xsi:type="sts:StringValue" id="FSM.name">
        <features xsi:type="ecore:EAttribute" href="http://fsm#//State/name"/>
      </subRules>
      <subRules xsi:type="sts:Terminal" id=";" terminal=";" separator="true"/>
      <subRules xsi:type="sts:Adornment" terminal="eoln"/>
    </rule>
  </rules>
  <rules xsi:type="sts:Template" id="Transition">
    <metaclass href="http://fsm#//Transition"/>
    <rule xsi:type="sts:Sequence">
      <subRules xsi:type="sts:Terminal" id="transition" terminal="transition"/>
      <subRules xsi:type="sts:Terminal" id="from" terminal="from"/>
      <subRules xsi:type="sts:ObjectReference" id="State.source">
        <features xsi:type="ecore:EReference" href="http://fsm#//Transition/source"/>
        <identifier xsi:type="ecore:EAttribute" href="http://fsm#//State/name"/>
      </subRules>
      <subRules xsi:type="sts:Terminal" id="to" terminal="to"/>
      <subRules xsi:type="sts:ObjectReference" id="State.target">
        <features xsi:type="ecore:EReference" href="http://fsm#//Transition/target"/>
        <identifier xsi:type="ecore:EAttribute" href="http://fsm#//State/name"/>
      </subRules>
      <subRules xsi:type="sts:Terminal" id="input" terminal="input"/>
      <subRules xsi:type="sts:StringValue" id="Transition.input">
        <features xsi:type="ecore:EAttribute" href="http://fsm#//Transition/input"/>
      </subRules>
      <subRules xsi:type="sts:Terminal" id="output" terminal="output"/>
      <subRules xsi:type="sts:StringValue" id="Transition.output">
        <features xsi:type="ecore:EAttribute" href="http://fsm#//Transition/output"/>
      </subRules>
      <subRules xsi:type="sts:Terminal" id=";" terminal=";" separator="true"/>
      <subRules xsi:type="sts:Adornment" terminal="eoln"/>
    </rule>
  </rules>
</sts:Root>
