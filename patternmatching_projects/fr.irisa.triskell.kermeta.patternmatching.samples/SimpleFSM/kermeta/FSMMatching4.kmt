@mainClass "fsm::FSMMatching4"
@mainOperation "main"


package fsm;

require kermeta
require "./FSMMatching.kmt"
 
using kermeta::standard 
 
class FSMMatching4 inherits FSMMatching
{
    operation main() : Void is do 
        var inputMPath : String init "platform:/resource/fr.irisa.triskell.kermeta.patternmatching.samples/SimpleFSM/models/FSMM.xmi"                              
        var patternPath : String init "platform:/resource/fr.irisa.triskell.kermeta.patternmatching.samples/SimpleFSM/models/FSMMPattern4_frame.xmi"                               
        
        executePatternMatching (inputMPath, patternPath )    
    end
}