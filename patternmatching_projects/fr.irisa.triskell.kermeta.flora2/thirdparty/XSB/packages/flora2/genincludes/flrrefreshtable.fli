	  )).
	
?- assert((
    	    FLORA_SYMBOL('refresh_table_for')(WRAP_IMVDSIG(X,Y,Z,_Caller))
    	    :-  % was WRAP_IFDSIG. Was that a typo? should be WRAP_IMVDSIG?
	    	    !,
	    	    flora_abolish_table_call(WRAP_IMVDSIG(X,Y,Z,_Caller)),
	    	    flora_abolish_table_call(WRAP_MVDSIG(X,Y,Z,_Caller))
	  )).

