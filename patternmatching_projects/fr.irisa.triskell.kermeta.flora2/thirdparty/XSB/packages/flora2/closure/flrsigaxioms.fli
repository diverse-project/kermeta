/* Axioms for signatures */

mvdsig(O,M,R1,CallerInfo) :-
	mvdsig(O,M,R,CallerInfo),
	sub(R,R1,CallerInfo).

imvdsig(C,M,R1,CallerInfo) :-
	imvdsig(C,M,R,CallerInfo),
	sub(R,R1,CallerInfo).


