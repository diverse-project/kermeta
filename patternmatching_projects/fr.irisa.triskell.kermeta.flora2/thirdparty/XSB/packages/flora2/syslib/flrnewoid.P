:- compiler_options([xpp_on]).
#include "flora_terms.flh"

%% Dynamic generation of new oid

:- export FLLIBNEWOID/1.
:- import gensym/2 from gensym.


%% May need to be more sophisticated here
FLLIBNEWOID(NewOid) :-
	var(NewOid),
	!,
	gensym(FL_NEWOID_DYNPREFIX,NewOid).
FLLIBNEWOID(_NewOid).
