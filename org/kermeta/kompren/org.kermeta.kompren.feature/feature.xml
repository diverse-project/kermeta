<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.kermeta.kompren.feature"
      label="Kompren"
      version="${bundle.version}"
      provider-name="IRISA/INRIA">

   <description url="http://people.irisa.fr/Arnaud.Blouin/software_kompren.html">
      Kompren is a DSML to model model slicers for a particular domain (captured in a metamodel).
   </description>

