   <url>
      <discovery label="Kermeta 2 update site" url="http://www.kermeta.org/k2/update/"/>
   </url>
   
   <requires>
      <import plugin="org.kermeta.language.model"/>
      <import plugin="org.scala-ide.scala.library"/>
      <import plugin="org.kermeta.language.library.core"/>
   </requires>

   <plugin
         id="org.kermeta.kompren.slicer.parser"
         download-size="0"
         install-size="0"
         version="0.0.0"/>

   <plugin
         id="org.kermeta.kompren.slicer.texteditor"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.kermeta.kompren.slicer.model"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.kermeta.kompren.slicer.edit"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.kermeta.kompren.slicer.editor"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.kermeta.kompren.slicer.compiler"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
