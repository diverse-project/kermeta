aspect class ModelType {
	/** Create html code */
	operation createHtml (baseFolder:String) : String is do
			var html : String init String.new	
			html := html + " <a href= " +self.name + ".html target = Content> " + self.name + "  </a>" + " <br>"
			result := html
    end
}

