<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="ca"
    nsURI="http://ca/1.0" nsPrefix="ca">
  <eClassifiers xsi:type="ecore:EClass" name="Rule">
    <eStructuralFeatures xsi:type="ecore:EReference" name="nextValue" lowerBound="1"
        eType="#//IntegerExpression" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="lowerRange" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="upperRange" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Max" eSuperTypes="#//CollToIntExp"/>
  <eClassifiers xsi:type="ecore:EClass" name="Minus" eSuperTypes="#//BinaryIntToIntExp"/>
  <eClassifiers xsi:type="ecore:EClass" name="Add" eSuperTypes="#//BinaryIntToIntExp"/>
  <eClassifiers xsi:type="ecore:EClass" name="IntegerExpression"/>
  <eClassifiers xsi:type="ecore:EClass" name="IntegerCollectionExpression"/>
  <eClassifiers xsi:type="ecore:EClass" name="BooleanExpression"/>
  <eClassifiers xsi:type="ecore:EClass" name="And" eSuperTypes="#//BinaryBoolToBoolExp"/>
  <eClassifiers xsi:type="ecore:EClass" name="Or" eSuperTypes="#//BinaryBoolToBoolExp"/>
  <eClassifiers xsi:type="ecore:EClass" name="Not" eSuperTypes="#//BooleanExpression">
    <eStructuralFeatures xsi:type="ecore:EReference" name="target" lowerBound="1"
        eType="#//BooleanExpression" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Greater" eSuperTypes="#//BinaryIntToBoolExp"/>
  <eClassifiers xsi:type="ecore:EClass" name="Lower" eSuperTypes="#//BinaryIntToBoolExp"/>
  <eClassifiers xsi:type="ecore:EClass" name="IntegerLiteral" eSuperTypes="#//IntegerExpression">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Conditional" eSuperTypes="#//IntegerExpression">
    <eStructuralFeatures xsi:type="ecore:EReference" name="ifTrueExpression" lowerBound="1"
        eType="#//IntegerExpression" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="ifFalseExpression" eType="#//IntegerExpression"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="condition" lowerBound="1"
        eType="#//BooleanExpression" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="SelfValue" eSuperTypes="#//IntegerExpression"/>
  <eClassifiers xsi:type="ecore:EClass" name="Size" eSuperTypes="#//CollToIntExp"/>
  <eClassifiers xsi:type="ecore:EClass" name="CollToIntExp" eSuperTypes="#//IntegerExpression">
    <eStructuralFeatures xsi:type="ecore:EReference" name="inputCollection" lowerBound="1"
        eType="#//IntegerCollectionExpression" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Min" eSuperTypes="#//CollToIntExp"/>
  <eClassifiers xsi:type="ecore:EClass" name="BinaryIntToIntExp" eSuperTypes="#//IntegerExpression">
    <eStructuralFeatures xsi:type="ecore:EReference" name="left" lowerBound="1" eType="#//IntegerExpression"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="right" lowerBound="1" eType="#//IntegerExpression"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Mult" eSuperTypes="#//BinaryIntToIntExp"/>
  <eClassifiers xsi:type="ecore:EClass" name="Div" eSuperTypes="#//BinaryIntToIntExp"/>
  <eClassifiers xsi:type="ecore:EClass" name="BinaryIntToBoolExp" eSuperTypes="#//BooleanExpression">
    <eStructuralFeatures xsi:type="ecore:EReference" name="left" lowerBound="1" eType="#//IntegerExpression"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="right" lowerBound="1" eType="#//IntegerExpression"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="BinaryBoolToBoolExp" eSuperTypes="#//BooleanExpression">
    <eStructuralFeatures xsi:type="ecore:EReference" name="left" lowerBound="1" eType="#//BooleanExpression"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="right" lowerBound="1" eType="#//BooleanExpression"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Xor" eSuperTypes="#//BinaryBoolToBoolExp"/>
  <eClassifiers xsi:type="ecore:EClass" name="NeighborsCollectionLiteral" eSuperTypes="#//IntegerCollectionExpression"/>
  <eClassifiers xsi:type="ecore:EClass" name="IntegerCollectionLiteral" eSuperTypes="#//IntegerCollectionExpression">
    <eStructuralFeatures xsi:type="ecore:EReference" name="values" upperBound="-1"
        eType="#//IntegerExpression" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Sum" eSuperTypes="#//CollToIntExp"/>
  <eClassifiers xsi:type="ecore:EClass" name="CellularAutomata">
    <eStructuralFeatures xsi:type="ecore:EReference" name="ownedRules" upperBound="-1"
        eType="#//Rule" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="geometry" eType="#//Geometry"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="appliedRules" lowerBound="1"
        upperBound="-1" eType="#//Rule"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="initialValue" eType="#//IntegerExpression"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Geometry" abstract="true"/>
  <eClassifiers xsi:type="ecore:EClass" name="RegularGeometry" eSuperTypes="#//Geometry">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="neighborsNumber" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="worldSize" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="dimension" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="isCircular" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="SelfGlobalPosition" eSuperTypes="#//IntegerExpression"/>
  <eClassifiers xsi:type="ecore:EClass" name="Mod" eSuperTypes="#//BinaryIntToIntExp"/>
</ecore:EPackage>
