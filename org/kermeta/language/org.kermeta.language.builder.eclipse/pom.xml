<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<artifactId>language.builder.eclipse</artifactId>
	<packaging>jar</packaging>
	<name>language.builder.eclipse :: kermeta builder</name>

    <parent>
        <artifactId>language.root</artifactId>
        <groupId>org.kermeta.language</groupId>
        <version>2.0.5-SNAPSHOT</version>
    </parent>
	<dependencies>
		<dependency>
			<groupId>org.osgi</groupId>
			<artifactId>org.osgi.core</artifactId>
			<version>${osgi.version}</version>
		</dependency>
		
        <dependency>
			<groupId>org.kermeta.utils</groupId>
			<artifactId>utils.helpers.eclipse</artifactId>
			<version>${utils.version}</version>
		</dependency>
		<dependency>
			<groupId>org.kermeta.eclipse</groupId>
			<artifactId>org.eclipse.core.commands</artifactId>
			<version>[3.5.0.0,)</version>
		</dependency>
		<dependency>
			<groupId>org.kermeta.eclipse</groupId>
			<artifactId>org.eclipse.core.jobs</artifactId>
			<version>[3.4.0.0,)</version>
		</dependency>
		<dependency>
			<groupId>org.kermeta.eclipse</groupId>
			<artifactId>org.eclipse.core.runtime</artifactId>
			<version>[3.5.0.0,)</version>
		</dependency>
		<!-- <dependency>
			<groupId>org.kermeta.eclipse</groupId>
			<artifactId>org.eclipse.core.resources</artifactId>
			<version>[3.5.2.0,)</version>
		</dependency>-->
		<dependency>
			<groupId>org.kermeta.eclipse</groupId>
			<artifactId>org.eclipse.equinox.common</artifactId>
			<version>[3.5.1.0,)</version>
		</dependency>
		<dependency>
			<groupId>org.kermeta.eclipse</groupId>
			<artifactId>org.eclipse.equinox.registry</artifactId>
			<version>[3.4.0.0,)</version>
		</dependency>
		<dependency>
			<groupId>org.kermeta.eclipse</groupId>
			<artifactId>org.eclipse.ui</artifactId>
			<version>[3.5.0.0,)</version>
		</dependency>
		<dependency>
			<groupId>org.kermeta.eclipse</groupId>
			<artifactId>org.eclipse.ui.ide</artifactId>
			<version>[3.5.0.0,)</version>
		</dependency>
		<dependency>
			<groupId>org.kermeta.eclipse</groupId>
			<artifactId>org.eclipse.ui.views</artifactId>
			<version>[3.4.0.0,)</version>
		</dependency>
		<dependency>
			<groupId>org.kermeta.eclipse</groupId>
			<artifactId>org.eclipse.ui.workbench</artifactId>
			<version>[3.5.0.0,)</version>
		</dependency>
		<dependency>
			<groupId>org.kermeta.eclipse</groupId>
			<artifactId>org.eclipse.swt</artifactId>
			<version>[3.5.2.0,)</version>
		</dependency>
		<dependency>
			<groupId>org.kermeta.eclipse</groupId>
			<artifactId>org.eclipse.swt.gtk.linux.x86_64</artifactId>
			<version>[3.5.2.0,)</version>
		</dependency>
		<dependency>
			<groupId>org.kermeta.eclipse</groupId>
			<artifactId>org.eclipse.jface</artifactId>
			<version>[3.5.0.0,)</version>
		</dependency>
		<dependency>
			<groupId>org.kermeta.eclipse</groupId>
			<artifactId>org.eclipse.jface.text</artifactId>
			<version>[3.5.2.0,)</version>
		</dependency>
		<dependency>
			<groupId>org.kermeta.eclipse</groupId>
			<artifactId>org.eclipse.ui.editors</artifactId>
			<version>[3.5.0.0,)</version>
		</dependency>
		<dependency>
			<groupId>org.kermeta.eclipse</groupId>
			<artifactId>org.eclipse.text</artifactId>
			<version>[3.5.0.0,)</version>
		</dependency>
		<dependency>
			<groupId>org.kermeta.eclipse</groupId>
			<artifactId>org.eclipse.ui.workbench.texteditor</artifactId>
			<version>[3.5.0.0,)</version>
		</dependency>
		<dependency>
			<groupId>${project.groupId}</groupId>
			<artifactId>language.builder.api</artifactId>
			<version>${kermeta.version}</version>
		</dependency>
		<dependency>
			<groupId>org.kermeta.utils</groupId>
			<artifactId>utils.systemservices.eclipse</artifactId>
			<version>${utils.version}</version>
		</dependency>
		<dependency>
			<groupId>org.kermeta.utils</groupId>
			<artifactId>utils.helpers</artifactId>
			<version>${utils.version}</version>
		</dependency>
		<dependency>
			<groupId>fr.irisa.triskell.utilities</groupId>
			<artifactId>cache.utilities</artifactId>
			<version>0.1.0</version>
		</dependency>
        <dependency>
            <groupId>org.kermeta.kp</groupId>
            <artifactId>kp.compiler.commandline</artifactId>
            <version>${project.version}</version>
            <exclusions>
                <exclusion>
                    <artifactId>maven-embedder-2.1-20080623-patched</artifactId>
                    <groupId>org.netbeans.external</groupId>
                </exclusion>
            </exclusions>
        </dependency>
        <dependency>
			<groupId>org.kermeta.language</groupId>
			<artifactId>language.texteditor.eclipse.nature</artifactId>
			<version>${kermeta.version}</version>
		</dependency>
	</dependencies>
	<build>
		<!-- Add resources from the plugin structure -->
        <resources>
	      	<resource>
	        	<directory>.</directory>
	        	<includes>
	          		<include>plugin.xml</include>
	        	</includes>
	      	</resource>
	      	<resource>
	      		<directory>schema</directory>
	      		<targetPath>schema</targetPath>
	      	</resource>
	    </resources>
		<sourceDirectory>src/main/java</sourceDirectory>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
		        <artifactId>maven-jar-plugin</artifactId>
				<version>2.3.2</version>
				<configuration>
		          <archive>
		            <manifestEntries>
						<Manifest-Version>1.0</Manifest-Version>
						<Bundle-SymbolicName>org.kermeta.${project.artifactId};singleton:=true</Bundle-SymbolicName>
						<Bundle-Name>org.kermeta.${project.artifactId}</Bundle-Name>						
						<Bundle-Vendor>${bundle.vendor}</Bundle-Vendor>
						<Bundle-Version>${bundle.version}</Bundle-Version>
						<Bundle-ActivationPolicy>lazy</Bundle-ActivationPolicy>
						<Bundle-Localization>plugin</Bundle-Localization>
						<Bundle-ManifestVersion>2</Bundle-ManifestVersion>
						<Bundle-Activator>org.kermeta.language.builder.eclipse.Activator</Bundle-Activator>
						<Bundle-ActivationPolicy>lazy</Bundle-ActivationPolicy>	
						<Bundle-Version>${bundle.version}</Bundle-Version>					
						<Bundle-ClassPath>.</Bundle-ClassPath>
						<Export-Package>
							org.kermeta.language.builder.eclipse
						</Export-Package>
						<Require-Bundle>
							org.eclipse.core.runtime,
							org.eclipse.core.resources,
							org.eclipse.ui,
							org.eclipse.ui.workbench,
							org.eclipse.ui.ide,
							org.eclipse.equinox.registry,
							org.kermeta.language.framework.scala,
							org.kermeta.utils.systemservices.api,
							org.kermeta.utils.systemservices.eclipse,
							org.kermeta.kp.model,
							org.kermeta.kp.loader.kp,
							org.kermeta.kp.compiler.commandline,
							org.kermeta.language.model,
							org.kermeta.language.builder.api,
							org.kermeta.utils.helpers,
							org.kermeta.utils.helpers.eclipse,
							fr.irisa.triskell.cache.utilities,
							org.kermeta.language.checker.api,
							org.kermeta.language.checker,
							org.eclipse.emf.ecore,
							org.eclipse.emf.ecore.xmi,
							org.kermeta.diagnostic.model,
							org.kermeta.language.texteditor.eclipse.nature				
                        </Require-Bundle>
		            </manifestEntries>		            
		          </archive>
		        </configuration>
			</plugin>
			<plugin>
			    <groupId>org.apache.maven.plugins</groupId>
			    <artifactId>maven-eclipse-plugin</artifactId>
			    <version>2.8</version>
			    <!-- see http://maven.apache.org/plugins/maven-eclipse-plugin/eclipse-mojo.html for more information -->
			    <configuration>
			      <downloadSources>true</downloadSources>
			      <downloadJavadocs>true</downloadJavadocs>
			 	  <projectNameTemplate>org.kermeta.[artifactId]</projectNameTemplate>
			      <projectnatures>
			        <projectnature>org.scala-ide.sdt.core.scalanature</projectnature>
			        <projectnature>org.eclipse.jdt.core.javanature</projectnature>
			      </projectnatures>
			      <buildcommands>
			      </buildcommands>
			      <classpathContainers>
			        <classpathContainer>org.eclipse.jdt.launching.JRE_CONTAINER</classpathContainer>
			      </classpathContainers>
			      <excludes>
			      </excludes>
			      <sourceIncludes>
			        <sourceInclude>**/*.java</sourceInclude>
			      </sourceIncludes>
			      <useProjectReferences>false</useProjectReferences>
			    </configuration>
			  </plugin>
		</plugins>
	</build>
</project>