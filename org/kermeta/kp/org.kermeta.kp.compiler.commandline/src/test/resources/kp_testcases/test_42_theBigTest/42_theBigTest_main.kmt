using kermeta::standard

package root_package{
	class Main
	{
		
	
	    operation main() : Void is do 
	       root_package_enum::EnumTester.new.main()	
	       root_package_exception::ExceptionTester.new.main()
	       root_package_generic::GenericTester.new.main()	
	       root_package_lambda::LambdaTester.new.main()	
	       root_package_loop::LoopTester.new.main()	
	       root_package_multipleInheritance::MultipleInheritanceTester.new.main()
	       root_package_multiplicity::MultiplicityTester.new.main()
	       
	       kermeta::io::StdIO.writeln("The end")
	    end
	}
}