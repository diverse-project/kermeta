		assertTrue(i.isOff)
		assertTrue(!i.hasNext)
		/* do
			i.next
			test :=false
		rescue (e:kermeta::exceptions::IteratorIsOff)
		rescue (e:kermeta::exceptions::Exception)
			test:=false
		end */
		
		c.add("toto")
