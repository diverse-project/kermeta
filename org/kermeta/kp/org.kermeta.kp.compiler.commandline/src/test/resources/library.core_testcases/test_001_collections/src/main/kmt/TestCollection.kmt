		var c:Collection[NamedElement]:=CollectionStub[NamedElement].new
		var n:NamedElement
		n:=NamedElement.new
		n.name:="toto"
		c.add(n)
		n:=NamedElement.new
		n.name:="tutu"
		c.add(n)
		n:=NamedElement.new
		n.name:="titi"
		c.add(n)
		assertTrue(c.forAllCpl{x,y|(x.name!=y.name) or (x.oid==y.oid)})
	end 

	operation testExists():Void is do
    	c.add("toto")
    	c.add("tutu")
