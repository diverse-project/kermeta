			var a1 : pkg::A init pkg::A.new
			var b1 : pkg::B init pkg::B.new
			a1.b := b1
			var b2 : pkg::B
			b2 := b2.a.b
			var a2 : pkg::A
			a2 := b1.a
		end
	}
