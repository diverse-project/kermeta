using kermeta::standard
using kermeta::io::StdIO => stdio

package testPackage{
	class A{
		reference b:B # a
		reference bs : B[0..*] # as
		reference bss : B[0..*]#ass
	}
	class B{
		reference a:A # b
		reference as:A # bs
		reference ass:A[0..*]#bss
	}
	
	// used for complex generics opposite properties relations
	class C[G,H] {
		reference refD : D[H,G][0..*] #refC
	}
	
	class D[X,Y]{
		reference refC : C[Y,X] #refD
	}
}