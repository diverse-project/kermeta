@mainClass "p1::Main"
@mainOperation "main"


package p1;


require kermeta

class A {
	attribute myAtt : kermeta::standard::String

}

class Main
{
	operation main() : Void is do 
		
		var myA : A init A.new
		myA.myAtt := "my value"

		var assignedA : A 
		assignedA ?= myA
		stdio.writeln(assignedA.myAtt)	end
}