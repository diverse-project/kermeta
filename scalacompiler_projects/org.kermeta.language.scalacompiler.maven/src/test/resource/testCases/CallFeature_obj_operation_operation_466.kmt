@mainClass "p1::Main"
@mainOperation "main"


package p1;


require kermeta

class A {
	attribute ai : kermeta::standard::Integer
 	
 	operation initialize() : A is do
 		ai := 2
 		result := self
 	end
 	
 	operation print() : Void is do
 		stdio.writeln("print ai using self.initialize.operation : " + self.ai.toString)
 	end
}

class Main
{
	operation main() : Void is do
		var a : A init A.new
		a.ai := 2
		a.initialize.print
	end
}