@mainClass "p1::Main"
@mainOperation "main"

package p1;

require kermeta

class A {
	attribute ai : kermeta::standard::Integer
 	
 	operation setAttribute(elt : kermeta::standard::Integer) is do
 		ai := elt
 		stdio.writeln("calling super "+ai.toString)
 	end
}

class B inherits A{
	
	method setAttribute(elt : kermeta::standard::Integer) is do
 		super(elt)
 	end
}

class Main
{
	operation main() : Void is do
		var b : B init B.new
		b.setAttribute(2)
	end
}