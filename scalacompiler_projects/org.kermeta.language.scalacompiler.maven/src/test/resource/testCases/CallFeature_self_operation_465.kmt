@mainClass "p1::Main"
@mainOperation "main"


package p1;


require kermeta

class A {
	attribute ai : kermeta::standard::Integer
 	
 	operation printAi() : Void is do
 		stdio.writeln("print ai using self.operation : " + self.ai.toString)
 	end
 	
 	operation print() : Void is do
 		self.printAi
 	end
}

class Main
{
	operation main() : Void is do
		var a : A init A.new
		a.ai := 2
		a.print
	end
}