@mainClass "testOpCall::Test_opcall"
@mainOperation "operation1"

package testOpCall;

require kermeta

class Test_opcall {

    operation operation1() : kermeta::standard::Integer is do
        result := operation_asg(1234)
        stdio.writeln(result.toString)
    end

    operation operation_asg(in : kermeta::standard::Integer) : kermeta::standard::Integer is do
        result := in + 3
    end


}

