@mainClass "pkgConditionalParenthesis::ConditionalParenthesis"
@mainOperation "main"


package pkgConditionalParenthesis;


require kermeta
using kermeta::standard


class ConditionalParenthesis
{
	operation opFalse() : Boolean is do
		result := false
	end

	operation main() : Void is do
		var i : Integer 
		var b : Boolean
		var s : String
		
		b := true
		i := 1
		s := "coucou"
		
		if b then stdio.writeln("1success") else stdio.writeln("1fail") end
		if i==1 then stdio.writeln("2success") else stdio.writeln("2fail") end
		if s.equals("coucou") then stdio.writeln("3success") else stdio.writeln("3fail") end
		if true then stdio.writeln("3success") else stdio.writeln("3fail") end
		if not opFalse() then stdio.writeln("4success") else stdio.writeln("4fail") end
	end
}
