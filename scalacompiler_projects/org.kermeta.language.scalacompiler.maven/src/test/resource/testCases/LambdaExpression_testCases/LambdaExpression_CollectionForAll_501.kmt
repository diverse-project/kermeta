@mainClass "test::resource::testCases::LambdaExpression_testCases::Main"
@mainOperation "main"


package test::resource::testCases::LambdaExpression_testCases;


require kermeta
class Main
{
	attribute numbers : kermeta::standard::Integer [0..*]
	
	operation main() : Void is do 
		numbers.add(3)
		numbers.add(5)
		numbers.add(7)
		numbers.add(9)
		var allOdds : kermeta::standard::Boolean init kermeta::standard::Boolean.new 
		allOdds := numbers.forAll{n | n < 10}
		stdio.writeln(allOdds.toString)
	end
}