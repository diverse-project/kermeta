@mainClass "kmt_testcases::LambdaExpression_plus1_501"
@mainOperation "main"


package kmt_testcases;


require kermeta

class LambdaExpression_plus1_501
{
	operation main() : Void is do 
		var lf1 : <kermeta::standard::Integer->kermeta::standard::Integer>
		var res : kermeta::standard::Integer init 0
		lf1 := function { i : kermeta::standard::Integer | stdio.writeln(i.toString) self
			i }
		res := lf1(3) 
		
		stdio.writeln(res.toString)
	end
}