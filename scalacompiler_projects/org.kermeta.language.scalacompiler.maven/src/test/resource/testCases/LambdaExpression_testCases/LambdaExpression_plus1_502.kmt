@mainClass "test::interpreter::kmt_testcases::Main"
@mainOperation "main"


package test::interpreter::kmt_testcases;


require kermeta

class Main
{
	operation main() : Void is do 
		var lf1 : <kermeta::standard::Integer->kermeta::standard::Integer>
		var res : kermeta::standard::Integer
		lf1 := function { i : kermeta::standard::Integer | i.plus(1) }
		res := lf1(3)
		stdio.writeln(res.toString)
	end
}