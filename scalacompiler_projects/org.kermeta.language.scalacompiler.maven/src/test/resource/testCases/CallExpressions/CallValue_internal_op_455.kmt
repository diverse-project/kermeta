@mainClass "p1::Main"
@mainOperation "main"

package p1;

require kermeta

class Main
{
	operation main() : Void is do
		writeOutput()
	end
	
	operation writeOutput() is do
		stdio.writeln("Call for an internal operation successful")
		var a : kermeta::standard::Integer
		a := 4
	end
}