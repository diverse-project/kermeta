@mainClass "root_package::Main"
@mainOperation "main"


package root_package;


require kermeta
class Main
{
	operation main() : Void is do 
		var a : kermeta::standard::String
		var obj : kermeta::language::structure::Object init kermeta::language::structure::Object.new
		
		a ?= obj
		
		stdio.writeln("Assignment value = "+a)
	end
}