@mainClass "p1::Main"
@mainOperation "main"


package p1;


require kermeta

class A {
	attribute ai : kermeta::standard::Integer
 	
 	operation getAi() : kermeta::standard::String is do
 		result := "2"
 	end
}

class Main
{
	operation main() : Void is do
		stdio.writeln("result := " + new_A().getAi)
	end
	
	operation new_A() : A is do
		stdio.writeln("define result as new Class")
		result := A.new
	end
}