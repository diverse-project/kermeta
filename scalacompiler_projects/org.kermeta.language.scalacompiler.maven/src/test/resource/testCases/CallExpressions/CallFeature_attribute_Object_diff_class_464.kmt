@mainClass "p1::Main"
@mainOperation "main"


package p1;


require kermeta
class Main
{
	attribute a : A
	operation main() : Void is do
		a := A.new
		stdio.writeln("Test attribute diff class of value ="+a.toto)
	end
}

class A{
	operation toto() : kermeta::standard::String is do
		result := "toto"
	end
}