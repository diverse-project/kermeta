@mainClass "root_package::Main"
@mainOperation "main"


package root_package;


require kermeta
class Main
{
	operation main() : Void is do 
		var a : kermeta::standard::Boolean
		a := false or (false and true) or (false or true)
		stdio.writeln("Assignment value = "+a.toString)	
	end
}