@mainClass "p1::Main"
@mainOperation "main"

package p1;

require kermeta

class A {
	attribute ai : kermeta::standard::Integer
 	
 	operation getAttribute() : kermeta::standard::Integer is do
 		stdio.writeln("calling super "+ai.toString)
 		result := ai
 	end
}

class B inherits A{
	
	method getAttribute() : kermeta::standard::Integer is do
 		result := super()
 	end
}

class Main
{
	operation main() : Void is do
		var b : B init B.new
		b.ai := 2
		stdio.writeln("ai := " + b.getAttribute.toString)
	end
}