@mainClass "p1::Main"
@mainOperation "main"


package p1;


require kermeta
class Main
{
	operation main() : Void is do
		var a : kermeta::standard::String init ""
		a := a.plus("S").plus("c").plus("a").plus("l").plus("a")
		stdio.writeln("Test plus call on strings ="+a)
	end
}