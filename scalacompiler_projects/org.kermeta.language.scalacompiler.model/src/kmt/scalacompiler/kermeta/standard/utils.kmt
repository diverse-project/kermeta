package kermeta::utils;


require kermeta

using kermeta::standard
aspect class StringBuffer
{
	operation insert(pos:Integer,s1:String):String is do
		var s:String 
		s?= extern kermeta::standard::String.insert(self.toString,s1,pos)
		
		result:=s
	end
	
}