@mainClass "root_package::Main"
@mainOperation "main"


package root_package;


require kermeta
require "../Math.kmt"

class Main
{
	operation main() : Void is do 
		var math : Math::Math init Math::Math.new
		var input : kermeta::standard::Real init "0".toReal
		
		stdio.writeln(math.sin(input).toString)
	end
}