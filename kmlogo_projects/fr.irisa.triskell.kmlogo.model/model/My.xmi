<?xml version="1.0" encoding="ASCII"?>
<ASM:Block xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ASM="http://kmLogo.ecore/ASM">
  <instructions xsi:type="ASM:Clear"/>
  <instructions xsi:type="ASM:Clear"/>
  <instructions xsi:type="ASM:ProcDeclaration" name="square">
    <block>
      <instructions xsi:type="ASM:Repeat">
        <condition xsi:type="ASM:Constant" integerValue="4"/>
        <block>
          <instructions xsi:type="ASM:Forward">
            <steps xsi:type="ASM:Constant" integerValue="2"/>
          </instructions>
        </block>
      </instructions>
    </block>
  </instructions>
  <instructions xsi:type="ASM:Forward">
    <steps xsi:type="ASM:Constant" integerValue="10"/>
  </instructions>
  <instructions xsi:type="ASM:Repeat">
    <condition xsi:type="ASM:Constant" integerValue="4"/>
    <block>
      <instructions xsi:type="ASM:Forward">
        <steps xsi:type="ASM:Constant" integerValue="2"/>
      </instructions>
    </block>
  </instructions>
  <instructions xsi:type="ASM:While">
    <condition xsi:type="ASM:Constant" integerValue="4"/>
    <block>
      <instructions xsi:type="ASM:Forward">
        <steps xsi:type="ASM:Constant" integerValue="-5"/>
      </instructions>
    </block>
  </instructions>
  <instructions xsi:type="ASM:Forward">
    <steps xsi:type="ASM:Constant" integerValue="10"/>
  </instructions>
  <instructions xsi:type="ASM:Repeat">
    <condition xsi:type="ASM:Constant" integerValue="6"/>
    <block>
      <instructions xsi:type="ASM:Clear"/>
    </block>
  </instructions>
  <instructions xsi:type="ASM:If">
    <condition xsi:type="ASM:Constant"/>
    <thenPart>
      <instructions xsi:type="ASM:Clear"/>
    </thenPart>
    <elsePart/>
  </instructions>
  <instructions xsi:type="ASM:Block">
    <instructions xsi:type="ASM:Clear"/>
  </instructions>
  <instructions xsi:type="ASM:ProcDeclaration" name="spiral">
    <args name="size"/>
    <block>
      <instructions xsi:type="ASM:Forward">
        <steps xsi:type="ASM:ParameterCall" parameter="//@instructions.10/@args.0"/>
      </instructions>
    </block>
  </instructions>
</ASM:Block>
