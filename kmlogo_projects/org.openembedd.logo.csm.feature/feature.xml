         id="org.openembedd.logo.csm.tutorial"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.openembedd.wizards"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
