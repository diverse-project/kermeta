   </license>

   <url>
      <update label="OpenEmbeDD Update Site" url="http://openembedd.org/update"/>
   </url>

   <requires>
      <import plugin="org.eclipse.jface"/>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.core.resources"/>
      <import feature="fr.irisa.triskell.kermeta.graphical" version="1.2.1" match="greaterOrEqual"/>
