package org::kermeta::binarymerger;


require kermeta
using kermeta::standard
class UnarySelectionner
{
	/**
	 * Select or create a new object from the primary element and the aspect
	 * By default, select the primaryElement 
	 **/
	operation select(primaryElement : Object, aspectElement : Object) : Object is do
		result :=  primaryElement
	end
		
}